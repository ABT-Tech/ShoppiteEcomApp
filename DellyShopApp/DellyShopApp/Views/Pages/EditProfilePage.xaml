<?xml version="1.0" encoding="utf-8" ?><base:BasePage    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    x:Class="DellyShopApp.Views.Pages.EditProfilePage"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"     xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"     xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp" xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"
    BackgroundColor="#f5f5f5">    <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">        <StackLayout            Margin="10"                Padding="5"                Spacing="10">            <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Edit Profile"           />                   </StackLayout>        <ScrollView Margin="0,80,0,0">            <StackLayout Padding="30">                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="User Name"                                                TextColor="Black"                        FontAttributes="Bold"/>                                        <StackLayout Orientation="Horizontal">                    <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                        <StackLayout Orientation="Horizontal" >                            <Image Style="{DynamicResource ImageStyleEntryRegister}">                                <Image.Source>                                    <FontImageSource                                            FontFamily="{DynamicResource MaterialFontFamily}"                                            Glyph="{x:Static font:FontAwesomeIcons.User}"                                            Size="20"                                            Color="Chocolate" />                                </Image.Source>                            </Image>                            <renderers:BorderlessEntry x:Name="UserName"  MaxLength="15"   Style="{DynamicResource BorderlessEntry}" />                        </StackLayout>                    </Frame>                </StackLayout>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="EMail"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiEmail}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry Keyboard="Email" x:Name="EmailAddress"  Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="PhoneNumber"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiCall}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry Keyboard="Numeric" x:Name="PhoneNumber" MaxLength="10" Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Address"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame HeightRequest="100" Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <renderers:BorderlessEditor x:Name="Address" BackgroundColor="Transparent" TextColor="{DynamicResource EntryTextColor}" />                </Frame>                <Button                        Margin="20"                        BackgroundColor="{DynamicResource IconBgColor}"                        Clicked="Button_Clicked"                        Text="Edit Profile"                    CornerRadius="20"                    FontSize="20"                        TextColor="Black" />            </StackLayout>        </ScrollView>    </Grid></base:BasePage>                                                                              