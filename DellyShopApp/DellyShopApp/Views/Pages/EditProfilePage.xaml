<?xml version="1.0" encoding="utf-8" ?><base:BasePage    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    x:Class="DellyShopApp.Views.Pages.EditProfilePage"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"     xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"     xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp" xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5">    <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">        <StackLayout            Margin="30" >            <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Edit Profile" />            <ActivityIndicator x:Name="uploadIndicator" IsVisible="False" IsRunning="False" Color="Chocolate" />                    </StackLayout>        <ScrollView Margin="0,120,0,0">            <StackLayout Padding="30">                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change UserName"                                                TextColor="Black"                        FontAttributes="Bold"/>                                        <StackLayout Orientation="Horizontal">                    <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                        <StackLayout Orientation="Horizontal" >                            <Image Style="{DynamicResource ImageStyleEntryRegister}">                                <Image.Source>                                    <FontImageSource                                            FontFamily="{DynamicResource MaterialFontFamily}"                                            Glyph="{x:Static font:FontAwesomeIcons.User}"                                            Size="20"                                            Color="Chocolate" />                                </Image.Source>                            </Image>                            <renderers:BorderlessEntry x:Name="UserName"     Style="{DynamicResource BorderlessEntry}" />                        </StackLayout>                    </Frame>                </StackLayout>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change EMail"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiEmail}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry  x:Name="EmailAddress"  Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                          FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change PhoneNumber"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiCall}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry Keyboard="Numeric" x:Name="number" MaxLength="10" Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change Address"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                     <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.AddressBook}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                    <renderers:BorderlessEntry x:Name="address" Style="{DynamicResource BorderlessEntry}"  />                         </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change State"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                      <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.BrandPlaystation}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                    <renderers:BorderlessEntry x:Name="statename" Style="{DynamicResource BorderlessEntry}" />                </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change City"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                      <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.Home}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                    <renderers:BorderlessEntry x:Name="cityname"  Style="{DynamicResource BorderlessEntry}" />                          </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change ZipCode"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}"  BackgroundColor="#dcd6d0">                      <StackLayout Orientation="Horizontal">                                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.Barcode}"                                        Size="20"                                        Color="Chocolate" />                            </Image.Source>                        </Image>                    <renderers:BorderlessEntry Keyboard="Numeric" BackgroundColor="Transparent"  x:Name="zipcode" Style="{DynamicResource BorderlessEntry}" />                          </StackLayout>                </Frame>                                <Button                        Margin="20"                        BackgroundColor="#E6583D"                        Clicked="Button_Clicked"                        Text="Edit Profile"                        TextColor="White" />            </StackLayout>        </ScrollView>    </Grid></base:BasePage>                                                                              