<?xml version="1.0" encoding="utf-8" ?><base:BasePage    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    x:Class="DellyShopApp.Views.Pages.EditProfilePage"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"     xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"            xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"     xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp" xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5">    <Grid HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">               <ScrollView>                        <StackLayout Margin="20,10,20,10">                <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Edit Profile"/>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change UserName"                                                TextColor="Black"                        FontAttributes="Bold"/>                <StackLayout Orientation="Horizontal">                    <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                        <StackLayout Orientation="Horizontal" >                            <Image Style="{DynamicResource ImageStyleEntryRegister}">                                <Image.Source>                                    <FontImageSource                                            FontFamily="{DynamicResource MaterialFontFamily}"                                            Glyph="{x:Static font:FontAwesomeIcons.User}"                                            Size="20"                                            Color="#ff002c" />                                </Image.Source>                            </Image>                            <renderers:BorderlessEntry x:Name="UserName"  MaxLength="25"   Style="{DynamicResource BorderlessEntry}" />                        </StackLayout>                    </Frame>                </StackLayout>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change EMail"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiEmail}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry  x:Name="EmailAddress"  Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                          FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change PhoneNumber"                        TextColor="Black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.UiCall}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry Keyboard="Numeric" x:Name="number" MaxLength="10" Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change Address"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.AddressBook}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry x:Name="address" Style="{DynamicResource BorderlessEntry}"  />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Change State"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.BrandPlaystation}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry x:Name="statename" Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change City"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}" BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.Home}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry x:Name="cityname"  Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text=" Change ZipCode"                        TextColor="black"                        FontAttributes="Bold"/>                <Frame  Style="{DynamicResource FrameStyle}"  BackgroundColor="#dcd6d0">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntryRegister}">                            <Image.Source>                                <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.Barcode}"                                        Size="20"                                        Color="#ff002c" />                            </Image.Source>                        </Image>                        <renderers:BorderlessEntry BackgroundColor="Transparent" MaxLength="6"  x:Name="zipcode" Style="{DynamicResource BorderlessEntry}" />                    </StackLayout>                </Frame>                <StackLayout VerticalOptions="EndAndExpand">                    <Frame                    Margin="20"                    BackgroundColor="#ff002c"                    Style="{DynamicResource ButtonFrameStyle}">                        <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="Button_Clicked" />                        </Frame.GestureRecognizers>                        <Label  xct:TouchEffect.AnimationDuration="250"                                                               xct:TouchEffect.PressedScale="0.75"                                                               xct:TouchEffect.PressedOpacity="0.6"                            Style="{DynamicResource ButtonLabelStyle}"                            Text="Update Profile"                            TextColor="White">                            <Label.GestureRecognizers>                                <TapGestureRecognizer Tapped="Button_Clicked" />                            </Label.GestureRecognizers>                        </Label>                    </Frame>                </StackLayout>                           </StackLayout>        </ScrollView>    </Grid></base:BasePage>                                                                              