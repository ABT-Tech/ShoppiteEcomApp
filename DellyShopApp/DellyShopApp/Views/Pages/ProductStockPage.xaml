<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.ProductStockPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp" xmlns:renderers="clr-namespace:DellyShopApp.Renderers"    iOsSpecific:Page.UseSafeArea="True"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">    <ScrollView>        <StackLayout Margin="10">            <StackLayout>                <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Products Stock" />            </StackLayout>            <customControl:RepeaterView                x:Name="BasketItems"                Margin="0,0,0,10"                Padding="2"                ItemsSource="{Binding ProcutListModel}">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell>                            <StackLayout>                                <pancakeView:PancakeView                                Margin="0,10,0,0"                                HeightRequest="100"                                Style="{DynamicResource PancakeFrameStyle}">                                    <StackLayout>                                        <StackLayout                                        BackgroundColor="{DynamicResource ItemColor}"                                        Orientation="Horizontal"                                        VerticalOptions="FillAndExpand">                                                                                       <pancakeView:PancakeView Padding="0"                                                              HorizontalOptions="Start"                                                              VerticalOptions="FillAndExpand"                                                              Style="{DynamicResource PancakeFrameStyle}">                                                <Image                                            Aspect="AspectFit"                                             HeightRequest="50"                                            Source="{Binding Image}"                                            WidthRequest="50" />                                            </pancakeView:PancakeView>                                            <StackLayout VerticalOptions="CenterAndExpand">                                                <Label                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="15"                                                LineBreakMode="TailTruncation"                                                Text="{Binding Title}"                                                TextColor="{DynamicResource WhiteDarkFontColor}"                                                WidthRequest="180" />                                                <Label                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="12"                                                Text="{Binding Brand}"                                                TextColor="{DynamicResource DescLabelColor}" />                                            </StackLayout>                                                                                    </StackLayout>                                        <StackLayout                                        Margin="5,0,5,0"                                        Orientation="Horizontal"                                        VerticalOptions="CenterAndExpand">                                            <StackLayout.GestureRecognizers>                                                <TapGestureRecognizer Tapped="OpenDetailClick" />                                            </StackLayout.GestureRecognizers>                                            <Label                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="15"                                            Text="{x:Static languages:AppResources.ShowMoreDetail}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Image                                            HorizontalOptions="EndAndExpand"                                            Rotation="{Binding Rotate}"                                            Style="{DynamicResource ImageStyleEntry}"                                            VerticalOptions="CenterAndExpand">                                                <Image.Source>                                                    <FontImageSource                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                                    Size="25"                                                    Color="{DynamicResource IconBgColor}" />                                                </Image.Source>                                            </Image>                                        </StackLayout>                                    </StackLayout>                                </pancakeView:PancakeView>                                <pancakeView:PancakeView                                Margin="0,10,0,10"                                Padding="7"                                HeightRequest="160"                                IsVisible="{Binding VisibleItemDelete}"                                Style="{DynamicResource PancakeFrameStyle}">                                    <StackLayout VerticalOptions="CenterAndExpand">                                        <StackLayout Orientation="Horizontal">                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="18"                                            HorizontalOptions="StartAndExpand"                                            Text="Price :-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Frame Style="{DynamicResource FrameStyle}"                                                    BackgroundColor="White"                                                   HeightRequest="40"                                                   WidthRequest="130"                                                   BorderColor="Black"                                                   HorizontalOptions="Start"                                                   VerticalOptions="Center">                                                <StackLayout Orientation="Horizontal">                                                    <renderers:BorderlessEntry  x:Name="price"                                                                                 IsEnabled="False"                                                                                BackgroundColor="Transparent"                                                                                Text="{Binding Price, StringFormat='{0}₹'}"                                                                                Keyboard="Numeric"                                                                                Style="{DynamicResource BorderlessEntry}" />                                                                                                    <Image Source="edit.png"                                                            Margin="10,0,10,0"                                                            HeightRequest="25"                                                            Style="{DynamicResource ImageStyleEntry}"                                                            WidthRequest="25">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer Tapped="PriceChange"/>                                                        </Image.GestureRecognizers>                                                    </Image>                                                </StackLayout>                                            </Frame>                                        </StackLayout>                                        <StackLayout Orientation="Horizontal">                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="StartAndExpand"                                            Text="Qty :-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Frame Style="{DynamicResource FrameStyle}"                                                     BackgroundColor="White"                                                     HeightRequest="40"                                                    WidthRequest="130"                                                                                                     BorderColor="Black"                                                    HorizontalOptions="Start"                                                   VerticalOptions="Center">                                                <StackLayout Orientation="Horizontal">                                                    <renderers:BorderlessEntry  Keyboard="Numeric"                                                                                BackgroundColor="Transparent"                                                                                Text="{Binding Quantity}"                                                                                IsEnabled="False"                                                                                  x:Name="Qty"                                                                                 Style="{DynamicResource BorderlessEntry}" />                                                    <Image Source="edit.png"                                                           Margin="10,0,10,0"                                                            HeightRequest="25"                                                            Style="{DynamicResource ImageStyleEntry}"                                                           WidthRequest="25">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer Tapped="QtyChange"/>                                                        </Image.GestureRecognizers>                                                    </Image>                                                </StackLayout>                                            </Frame>                                        </StackLayout>                                        <StackLayout  VerticalOptions="Start">                                            <Frame  Margin="0,0,0,0" HeightRequest="40"                                                     BackgroundColor="Chocolate"                                                      Style="{DynamicResource ButtonFrameStyle}">                                                <Frame.GestureRecognizers>                                                    <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped" />                                                 </Frame.GestureRecognizers>                                                <Label                                                      Style="{DynamicResource ButtonLabelStyle}"                                                      Text="Submit"                                                      FontSize="20"                                                      TextColor="{DynamicResource LabelTextColor}">                                                    <Label.GestureRecognizers>                                                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped" />                                                    </Label.GestureRecognizers>                                                </Label>                                            </Frame>                                        </StackLayout>                                    </StackLayout>                                </pancakeView:PancakeView>                            </StackLayout>                        </ViewCell>                    </DataTemplate>                                    </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>        </StackLayout>    </ScrollView></base:BasePage>  