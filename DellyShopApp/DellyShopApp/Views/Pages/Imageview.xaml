<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.Imageview"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:controls="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:customView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"    xmlns:proc="clr-namespace:DellyShopApp.Processors"    xmlns:local="clr-namespace:TestApp.CustomControl"    iOsSpecific:Page.UseSafeArea="False"    BackgroundColor="White"    NavigationPage.HasNavigationBar="False"    Visual="Material">    <AbsoluteLayout>        <StackLayout            Margin="15,60,15,0"            Padding="2"            AbsoluteLayout.LayoutBounds="1,1,1,1"            AbsoluteLayout.LayoutFlags="All">            <StackLayout Orientation="Horizontal">                <StackLayout HorizontalOptions="Start">                    <Image Aspect="AspectFit"                                   HeightRequest="40"                                   WidthRequest="40">                        <Image.Source>                            <FontImageSource                        FontFamily="{DynamicResource MaterialFontFamily}"                        Glyph="{x:Static font:FontAwesomeIcons.ThinLeft}"                        Size="50"                                   Color="#ff002c" />                        </Image.Source>                                          </Image>                </StackLayout>                                                                              </StackLayout>            </StackLayout>            <StackLayout                x:Name="NavbarStack"                BackgroundColor="{DynamicResource BackgroundColor}"                HorizontalOptions="FillAndExpand"                IsVisible="False"                Orientation="Horizontal"                VerticalOptions="Start">                                                                                          </StackLayout>                          <StackLayout>                                       <customView:PancakeView                        Margin="0,10,0,0"                        Padding="0"                        CornerRadius="8"                        Elevation="0"                        HasShadow="False">                        <customView:CarouselView                            x:Name="CarouselView"                            Margin="0,0,0,0"                            BackgroundColor="Transparent"                            HeightRequest="{x:OnIdiom Tablet=350,                                                      Phone=300}"                            SelectedIndex="0"                                                         ItemsSource="{Binding ProductList}"                            WidthRequest="300">                            <x:Arguments>                                <proc:BaseCarouselFrontViewProcessor                                    OpacityFactor="0.5"                                    RotationFactor="0"                                    ScaleFactor=".8" />                                <proc:BaseCarouselBackViewProcessor                                    OpacityFactor="0.5"                                    RotationFactor="0"                                    ScaleFactor=".8" />                            </x:Arguments>                            <customControl:IndicatorsControl SelectedIndicatorStyle="{StaticResource ActiveIndicator}" UnselectedIndicatorStyle="{DynamicResource InactiveIndicator}" />                            <customView:CarouselView.ItemTemplate>                                <DataTemplate>                                    <ContentView>                                        <forms:CachedImage                                            x:Name="showimg"                                            Margin="0"                                            Aspect="AspectFit"                                            Source="{Binding Image}"/>                                    </ContentView>                                </DataTemplate>                            </customView:CarouselView.ItemTemplate>                        </customView:CarouselView>                    </customView:PancakeView>                                    </StackLayout>    </AbsoluteLayout></base:BasePage>