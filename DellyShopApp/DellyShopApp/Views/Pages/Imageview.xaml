<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.Imageview"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:controls="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:customView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"    xmlns:proc="clr-namespace:DellyShopApp.Processors"    xmlns:local="clr-namespace:TestApp.CustomControl"    xmlns:pinch="clr-namespace:Xamarin.Forms.PinchZoomImage;assembly=Xamarin.Forms.PinchZoomImage"     iOsSpecific:Page.UseSafeArea="False"    BackgroundColor="White"    NavigationPage.HasNavigationBar="False"    Visual="Material">   <StackLayout>                            <Image Margin="5,20,0,0"            x:Name="BackButton"            Grid.Row="0"            Grid.Column="0"            Aspect="AspectFill"            HeightRequest="50"            HorizontalOptions="Start"            VerticalOptions="CenterAndExpand"            WidthRequest="50">                    <Image.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped"></TapGestureRecognizer>                    </Image.GestureRecognizers>                    <Image.Source>                        <FontImageSource                    FontFamily="{DynamicResource MaterialFontFamily}"                    Glyph="{x:Static font:FontAwesomeIcons.ThinLeft}"                    Size="50"                    Color="#ff002c" />                    </Image.Source>                </Image>                     <StackLayout HeightRequest="550" Margin="10,10,10,10" WidthRequest="600"                        HorizontalOptions="CenterAndExpand"                        VerticalOptions="CenterAndExpand">        <pinch:PinchZoom>            <pinch:PinchZoom.Content>        <Image Aspect="AspectFit"               HeightRequest="550"               WidthRequest="600"               Margin="0"               x:Name="showimg"               Source="{Binding Image}">        </Image>           </pinch:PinchZoom.Content></pinch:PinchZoom>    </StackLayout>   </StackLayout></base:BasePage>