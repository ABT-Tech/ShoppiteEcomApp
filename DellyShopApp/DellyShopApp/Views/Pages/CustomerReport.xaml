<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.CustomerReport"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp" xmlns:renderers="clr-namespace:DellyShopApp.Renderers"    iOsSpecific:Page.UseSafeArea="True"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">    <ScrollView>        <StackLayout Margin="10">            <StackLayout>                <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Report" />            </StackLayout>            <customControl:RepeaterView                x:Name="BasketItems"                Margin="0,0,0,10"                Padding="2"                ItemsSource="{Binding ProcutListModel}">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell>                            <StackLayout>                                                               <pancakeView:PancakeView                                Margin="0,10,0,5"                                Padding="7"                                HeightRequest="90"                                IsVisible="{Binding VisibleItemDelete}"                                Style="{DynamicResource PancakeFrameStyle}">                                    <StackLayout VerticalOptions="CenterAndExpand" Margin="10,0,10,0">                                        <StackLayout Orientation="Horizontal">                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="StartAndExpand"                                            Text="Name:-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="End"                                            Text="{Binding UserName}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                        <StackLayout Orientation="Horizontal">                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="StartAndExpand"                                            Text="Last Order Date :-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="End"                                            Text="{Binding Date}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                        <StackLayout Orientation="Horizontal">                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="StartAndExpand"                                            Text="Total Orders :-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="15"                                            HorizontalOptions="End"                                            Text="{Binding TtlOrder}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                                                           </StackLayout>                                </pancakeView:PancakeView>                            </StackLayout>                        </ViewCell>                    </DataTemplate>                </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>        </StackLayout>    </ScrollView></base:BasePage>  