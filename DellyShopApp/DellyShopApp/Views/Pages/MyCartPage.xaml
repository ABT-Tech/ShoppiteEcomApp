<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.MyCartPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"    iOsSpecific:Page.UseSafeArea="True"     xmlns:languages="clr-namespace:DellyShopApp.Languages"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">    <ScrollView>        <StackLayout Margin="10" x:Name="MainLayout" >            <ActivityIndicator x:Name="uploadIndicator"
                               IsVisible="False" 
                               IsRunning="False" 
                               Color="Chocolate" />
            <Grid HorizontalOptions="FillAndExpand" Margin="0,10,0,10">                <Grid.RowDefinitions>                    <RowDefinition Height="*" />                </Grid.RowDefinitions>                <Grid.ColumnDefinitions>                    <ColumnDefinition Width="50" />                    <ColumnDefinition Width="*" />                    <ColumnDefinition Width="50" />                </Grid.ColumnDefinitions>                <Image            x:Name="BackButton"            Grid.Row="0"            Grid.Column="0"            Aspect="AspectFill"            HeightRequest="50"            HorizontalOptions="Start"            VerticalOptions="CenterAndExpand"            WidthRequest="50">                    <Image.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2"></TapGestureRecognizer>                    </Image.GestureRecognizers>                    <Image.Source>                        <FontImageSource                    FontFamily="{DynamicResource MaterialFontFamily}"                    Glyph="{x:Static font:FontAwesomeIcons.ThinLeft}"                    Size="50"                    Color="Chocolate" />                    </Image.Source>                </Image>                <Label            x:Name="Title"            Grid.Row="0"            Grid.Column="1"            Margin="0,0,0,0"            FontFamily="{DynamicResource VerdanaProRegular}"            FontSize="22"            FontAttributes="Bold"            HorizontalOptions="CenterAndExpand"            Style="{StaticResource LabelStyle}"            LineBreakMode="TailTruncation"            Text="My Cart"            TextColor="Chocolate"            VerticalOptions="CenterAndExpand"            VerticalTextAlignment="Center" />            </Grid>            <customControl:RepeaterView x:Name="BasketItems">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell >                            <pancakeView:PancakeView Margin="0,5,0,0"  HeightRequest="110" Style="{DynamicResource PancakeFrameStyle}" >                                <pancakeView:PancakeView.GestureRecognizers>                                    <TapGestureRecognizer Tapped="ClickItem" />                                </pancakeView:PancakeView.GestureRecognizers>                                <SwipeView>                                    <SwipeView.RightItems>                                        <SwipeItems>                                            <SwipeItemView >                                                <Frame                                                         Margin="0,-5,0,-5"                                                        Padding="5"                                                        BackgroundColor="#FB5F5F"                                                        CornerRadius="10"                                                        HasShadow="False"                                                        IsVisible="true"                                                        WidthRequest="60">                                                    <Image                                                            Aspect="AspectFit"                                                            HeightRequest="30"                                                            HorizontalOptions="CenterAndExpand"                                                            VerticalOptions="CenterAndExpand"                                                            WidthRequest="30">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_1"></TapGestureRecognizer>                                                        </Image.GestureRecognizers>                                                        <Image.Source>                                                            <FontImageSource                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                                    Glyph="{x:Static font:FontAwesomeIcons.Close}"                                                                    Size="30"                                                                    Color="White" />                                                        </Image.Source>                                                    </Image>                                                </Frame>                                            </SwipeItemView>                                        </SwipeItems>                                    </SwipeView.RightItems>                                    <StackLayout                                         BackgroundColor="White"                                        Orientation="Horizontal"                                        VerticalOptions="Center">
                                        <pancakeView:PancakeView                                            Padding="0"                                            HorizontalOptions="End"                                            Style="{DynamicResource PancakeFrameStyle}"                                             VerticalOptions="FillAndExpand">                                            <Image                                         Aspect="AspectFit"                                        HeightRequest="60"                                        Source="{Binding Image}"                                        WidthRequest="100" />                                        </pancakeView:PancakeView>                                        <StackLayout Margin="10,0,10,0" >                                            <Label VerticalOptions="StartAndExpand"                                           HorizontalOptions="FillAndExpand"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="18"                                            LineBreakMode="TailTruncation"                                            Text="{Binding Title}"                                            MaxLines="1"                                            TextColor="{DynamicResource WhiteDarkFontColor}"/>                                    <Label VerticalOptions="CenterAndExpand"                                           HorizontalOptions="FillAndExpand"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="14"                                             LineBreakMode="TailTruncation"                                            MaxLines="1"                                            Text="{Binding Brand}"                                            TextColor="Gray" />                                            <Label VerticalOptions="CenterAndExpand"                                           HorizontalOptions="FillAndExpand"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="14"                                            Text="{Binding SpecificationNames}"                                             IsVisible="{Binding ISSpecificationNames}"                                            TextColor="Gray" />                                            <Label Margin="0,0,10,0"                                              FontAttributes="Bold"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                             FontSize="18"                                             HorizontalOptions="FillAndExpand"                                             Text="{Binding Price, StringFormat='{0} ₹'}"                                             TextColor="Black"                                             MaxLines="1"                                           IsVisible="{Binding IsPriceVisible}"                                          VerticalOptions="EndAndExpand" />                                                <Label Margin="0,0,10,0"                                             FontAttributes="Bold"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                             FontSize="18"                                                                                                    HorizontalOptions="FillAndExpand"                                                   Text="Out Of Stock!"                                             TextColor="Red"                                                        IsVisible="{Binding IsOutStock}"                                          VerticalOptions="EndAndExpand" />                                                                                   </StackLayout>                                        <StackLayout Orientation="Horizontal" HorizontalOptions="EndAndExpand" IsVisible="{Binding IsPriceVisible}">                                                        <Image  Margin="{x:OnIdiom Tablet='0,0,10,0',                                               Phone='0,0,10,0'}"                                                 Aspect="AspectFit"                                                HeightRequest="20"                                                HorizontalOptions="Start"                                                VerticalOptions="CenterAndExpand"                                                 WidthRequest="20">                                                            <Image.GestureRecognizers>                                                                <TapGestureRecognizer Tapped="MinusClick" />                                                            </Image.GestureRecognizers>                                                            <Image.Source>                                                                <FontImageSource                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                    Glyph="{x:Static font:FontAwesomeIcons.Minus}"                                                    Size="10"                                                    Color="#E59500" />                                                            </Image.Source>                                                        </Image>                                                        <Label WidthRequest="35"                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="20"                                             HorizontalOptions="Center"                                            Text="{Binding Quantity}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="Center" />                                                        <Label WidthRequest="35"                                            FontFamily="{DynamicResource VerdanaProLight}"                                             FontSize="20"                                                 HorizontalOptions="Center"                                                Text="{Binding Id}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                                IsVisible="False"/>                                                        <Image                                     Margin="{x:OnIdiom Tablet='0,0,10,0',                                               Phone='0,0,10,0'}"                                            Aspect="AspectFit"                                            HeightRequest="20"                                             HorizontalOptions="End"                                            VerticalOptions="CenterAndExpand"                                             WidthRequest="20">                                                            <Image.GestureRecognizers>                                                                <TapGestureRecognizer Tapped="PlusClick" />                                                            </Image.GestureRecognizers>                                                            <Image.Source>                                                                <FontImageSource                                                FontFamily="{DynamicResource MaterialFontFamily}"                                                Glyph="{x:Static font:FontAwesomeIcons.Plus}"                                                Size="10"                                                Color="#E59500" />                                                            </Image.Source>                                                        </Image>                                                    </StackLayout>                                        </StackLayout>                                                                           </SwipeView>                            </pancakeView:PancakeView>                        </ViewCell>                    </DataTemplate>                </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>            <Image Source="emptycart.gif" HeightRequest="300" WidthRequest="500"                   HorizontalOptions="Center"                   VerticalOptions="Start"                   x:Name="gif" ></Image>                       <StackLayout  VerticalOptions="EndAndExpand" Margin="20"  x:Name="shopping">                <!--<Frame                    Margin="0,0,0,20"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueWithPaypal" />                    </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Paytm"                                    Size="20"                                    Color="{DynamicResource LabelTextColor}" />                            </Image.Source>                        </Image>                </Frame>-->                <Frame                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2" />                    </Frame.GestureRecognizers>                    <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Continue Shopping"                        FontSize="20"                        TextColor="Black">                        <Label.GestureRecognizers>                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2" />                        </Label.GestureRecognizers>                    </Label>                </Frame>            </StackLayout>            <StackLayout x:Name="checkout" Margin="20" VerticalOptions="EndAndExpand">                <Frame                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="Button_Clicked" />                    </Frame.GestureRecognizers>                    <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                        <Image.Source>                            <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Process To CheckOut"                                    Size="20"                                    Color="Black" />                        </Image.Source>                    </Image>                </Frame>            </StackLayout>            <Image Source="basket12.png" HeightRequest="200" WidthRequest="200"                   HorizontalOptions="Center"                   VerticalOptions="CenterAndExpand"                   x:Name="cartimg" ></Image>            <Label                     Text="Empty Basket"                       HorizontalOptions="Center"                   VerticalOptions="CenterAndExpand"                       x:Name="txt"                       FontSize="20"/>            <pancakeView:PancakeView                   Margin="0,10,0,0"                    BackgroundColor="#dcd6d0"                    BackgroundGradientAngle="100"                    VerticalOptions="StartAndExpand"                    BackgroundGradientEndColor="White"                    BackgroundGradientStartColor="White"                    HasShadow="True"                    x:Name="Login"                    Style="{DynamicResource PancakeFrameStyle}">            <StackLayout                    Orientation="Horizontal"                    VerticalOptions="Center">                <StackLayout.GestureRecognizers>                    <TapGestureRecognizer Tapped="LogInClick" />                </StackLayout.GestureRecognizers>                <Image                        Margin="10,0,10,0"                        Aspect="AspectFit"                        HeightRequest="30"                        HorizontalOptions="Start"                        VerticalOptions="StartAndExpand"                        WidthRequest="30">                    <Image.Source>                        <FontImageSource                                FontFamily="{DynamicResource MaterialFontFamily}"                                Glyph="{x:Static font:FontAwesomeIcons.Login}"                                Size="30"                                Color="{DynamicResource IconBgColor}" />                    </Image.Source>                </Image>                    <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="20"                        Style="{DynamicResource ProfleLabelStyle}"                        Text="Login"                        TextColor="{DynamicResource WhiteDarkFontColor}"                        VerticalOptions="CenterAndExpand" />                </StackLayout>            </pancakeView:PancakeView>            <pancakeView:PancakeView                    Margin="0,10,0,250"                     BackgroundColor="#dcd6d0"                     BackgroundGradientAngle="100"                                       BackgroundGradientEndColor="White"                     BackgroundGradientStartColor="White"                     HasShadow="True"                    x:Name="vendorlogin"                    Style="{DynamicResource PancakeFrameStyle}">                <StackLayout                    Orientation="Horizontal"                    VerticalOptions="FillAndExpand">                    <StackLayout.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped" />                    </StackLayout.GestureRecognizers>                    <Image                        Margin="10,0,10,0"                        Aspect="AspectFit"                        HeightRequest="30"                        HorizontalOptions="Start"                        VerticalOptions="CenterAndExpand"                        WidthRequest="30">                        <Image.Source>                            <FontImageSource                                FontFamily="{DynamicResource MaterialFontFamily}"                                Glyph="{x:Static font:FontAwesomeIcons.Login}"                                Size="30"                                Color="{DynamicResource IconBgColor}" />                        </Image.Source>                    </Image>                    <Label                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="20"                        Style="{DynamicResource ProfleLabelStyle}"                        Text="Vendor Login"                        TextColor="{DynamicResource WhiteDarkFontColor}"                        VerticalOptions="CenterAndExpand" />                </StackLayout>            </pancakeView:PancakeView>        </StackLayout>    </ScrollView></base:BasePage> 