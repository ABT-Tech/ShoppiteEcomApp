<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.MyFavoritePage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"    iOsSpecific:Page.UseSafeArea="True"     xmlns:languages="clr-namespace:DellyShopApp.Languages"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">      <ScrollView>    <StackLayout Margin="10">        <ActivityIndicator x:Name="uploadIndicator" IsVisible="False" IsRunning="False" Color="Chocolate" />         <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="{x:Static languages:AppResources.MyFavorites}"/>                            <customControl:RepeaterView x:Name="BasketItems">            <customControl:RepeaterView.ItemTemplate>                <DataTemplate>                    <ViewCell>                        <pancakeView:PancakeView Margin="0,10,0,0"                                                 Style="{DynamicResource PancakeFrameStyle}">                            <pancakeView:PancakeView.GestureRecognizers>                                <TapGestureRecognizer Tapped="ClickItem" />                            </pancakeView:PancakeView.GestureRecognizers>                            <StackLayout                                BackgroundColor="{DynamicResource ItemColor}"                                Orientation="Horizontal"                                VerticalOptions="FillAndExpand">                                <Image                                    HeightRequest="25"                                    HorizontalOptions="End"                                    Rotation="{Binding Rotate}"                                    Style="{DynamicResource ImageStyleEntry}"                                    VerticalOptions="CenterAndExpand"                                    WidthRequest="25">                                    <Image.Source>                                        <FontImageSource                                            FontFamily="{DynamicResource MaterialFontFamily}"                                            Glyph="{x:Static font:FontAwesomeIcons.Heart}"                                            Size="25"                                            Color="Red" />                                    </Image.Source>                                </Image>                                <pancakeView:PancakeView                                    HeightRequest="70"                                    Padding="0"                                    HorizontalOptions="Start"                                    Style="{DynamicResource PancakeFrameStyle}"                                    VerticalOptions="FillAndExpand">                                    <Image                                        Aspect="AspectFit"                                        HeightRequest="50"                                        Source="{Binding Image}"                                        WidthRequest="50" />                                </pancakeView:PancakeView>                                <StackLayout                                        HorizontalOptions="StartAndExpand"                                        VerticalOptions="EndAndExpand"                                        WidthRequest="100">                                    <Label                                        FontFamily="{DynamicResource VerdanaProRegular}"                                        FontSize="15"                                        LineBreakMode="TailTruncation"                                        MaxLines="1"                                        Text="{Binding Title}"                                        TextColor="{DynamicResource WhiteDarkFontColor}"                                       />                                      <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding Brand}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                    <Label                                        FontFamily="{DynamicResource VerdanaProRegular}"                                        FontSize="12"                                        IsVisible="{Binding IsSpecificationNames}"                                        Text="{Binding SpecificationNames}"                                        TextColor="{DynamicResource DescLabelColor}" />                                </StackLayout>                                <Label                                    Margin="0,0,10,0"                                    FontAttributes="Bold"                                    FontFamily="{DynamicResource VerdanaProRegular}"                                    FontSize="20"                                    HorizontalOptions="EndAndExpand"                                    Text="{Binding Price, StringFormat='{0}₹'}"                                    TextColor="Black"                                    VerticalOptions="CenterAndExpand" />                            </StackLayout>                        </pancakeView:PancakeView>                    </ViewCell>                </DataTemplate>            </customControl:RepeaterView.ItemTemplate>        </customControl:RepeaterView>               <Image Source="emptywishlist.png" HeightRequest="300" WidthRequest="500"                   HorizontalOptions="Center" IsVisible="false"                   VerticalOptions="EndAndExpand" Margin="10"                   x:Name="gif" ></Image>            <StackLayout  VerticalOptions="EndAndExpand" Margin="20"  x:Name="shopping" IsVisible="false">                               <Frame                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2" />                    </Frame.GestureRecognizers>                    <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Continue Shopping"                        FontSize="20"                        TextColor="Black">                        <Label.GestureRecognizers>                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2" />                        </Label.GestureRecognizers>                    </Label>                </Frame>            </StackLayout>    </StackLayout>    </ScrollView></base:BasePage>