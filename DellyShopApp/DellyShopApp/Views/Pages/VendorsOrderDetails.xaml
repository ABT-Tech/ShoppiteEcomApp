<?xml version="1.0" encoding="utf-8" ?><base:BasePage     x:Class="DellyShopApp.Views.TabbedPages.VendorsOrderDetails"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:helpers="clr-namespace:DellyShopApp.Languages"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Material">    <ContentPage.Content>        <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">            <StackLayout                x:Name="MainStack"                Margin="10"                Padding="5"                Spacing="10">                <StackLayout                    Margin="0,10,0,0"                    HeightRequest="50"                    VerticalOptions="Start">                    <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Order Details"/>                                  </StackLayout>                <customControl:RepeaterView x:Name="BasketItems">                    <customControl:RepeaterView.ItemTemplate>                        <DataTemplate>                            <ViewCell>                                <pancakeView:PancakeView                                    Margin="0,10,0,0"                                    Padding="5"                                    Style="{DynamicResource PancakeFrameStyle}">                                    <pancakeView:PancakeView.GestureRecognizers>                                        <TapGestureRecognizer Tapped="ClickItem" />                                    </pancakeView:PancakeView.GestureRecognizers>                                    <!--  Content  -->                                    <StackLayout                                            BackgroundColor="{DynamicResource ItemColor}"                                            Orientation="Horizontal"                                            VerticalOptions="FillAndExpand">                                        <pancakeView:PancakeView                                                Padding="0"                                                HorizontalOptions="Start"                                                Style="{DynamicResource PancakeFrameStyle}"                                                VerticalOptions="FillAndExpand">                                            <Image                                                    Aspect="AspectFill"                                                    HeightRequest="50"                                                    Source="{Binding Image}"                                                    WidthRequest="50" />                                        </pancakeView:PancakeView>                                        <StackLayout HorizontalOptions="StartAndExpand" VerticalOptions="EndAndExpand" WidthRequest="200">                                            <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="15"                                                    Text="{Binding Title}" LineBreakMode="TailTruncation"                                                    TextColor="{DynamicResource WhiteDarkFontColor}" />                                            <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding Brand}"                                                    TextColor="Black" />                                        </StackLayout>                                        <Label                                                Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="17"                                                HorizontalOptions="End"                                                Text="{Binding Price, StringFormat='{0} ₹'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                        <Label                                                Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="15"                                                HorizontalOptions="End"                                                Text="{Binding Quantity, StringFormat='X {0}'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                    </StackLayout>                                </pancakeView:PancakeView>                            </ViewCell>                        </DataTemplate>                    </customControl:RepeaterView.ItemTemplate>                </customControl:RepeaterView>                <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="{x:Static helpers:AppResources.TotalPrice}"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPrice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal">                        <Label WidthRequest="150" VerticalOptions="Center"                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Order Status :-"                        TextColor="black"/>                        <renderers:BorderlessPicker                             Title="Select Status"                            TitleColor="Red"                             x:Name="PickerDemo"/>                        <Image>                            <Image.Source>                                <FontImageSource                                         FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.ThinDown}"                                        Size="40"                                        Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                <StackLayout Orientation="Horizontal" Margin="6">                                           <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="17" WidthRequest="150"                                            HorizontalOptions="StartAndExpand"                                            Text="Order Date :-"                                             TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                        <Label                                            x:Name="lblDate"                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="15"                                            HorizontalOptions="StartAndExpand"                                            Text="{Binding Date}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                  </StackLayout>                <StackLayout Orientation="Horizontal" Margin="6">                    <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="17" WidthRequest="350"                                            HorizontalOptions="StartAndExpand"                                            Text="Shipping Address :-"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                    <Label                                            x:Name="lblAddress"                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="15"                                            MaxLines="3"                                            HorizontalOptions="Start"                                            Text="{Binding Address}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                </StackLayout>                                <StackLayout Orientation="Horizontal" Margin="6">                    <Label WidthRequest="90" VerticalOptions="Center"                        FontFamily="{DynamicResource VerdanaProRegular}"                        FontSize="17"                        Style="{StaticResource LabelStyle}"                        Text="Remark :-"                        TextColor="black"                        FontAttributes="Bold"/>                    <Frame Style="{DynamicResource FrameStyle}"                            BackgroundColor="White"                           WidthRequest="250"                           BorderColor="Black"                                                     HorizontalOptions="Start">                        <StackLayout Orientation="Horizontal">                            <renderers:BorderlessEntry                                Placeholder="Type here..."                                Text="{Binding Remark}"                                BackgroundColor="Transparent"                                Style="{DynamicResource BorderlessEntry}" />                        </StackLayout>                    </Frame>                </StackLayout>                <StackLayout  VerticalOptions="Start">                    <Frame                    Margin="0,0,0,0"                    BackgroundColor="Chocolate"                    Style="{DynamicResource ButtonFrameStyle}">                        <!--<Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Frame.GestureRecognizers>-->                        <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Submit"                        FontSize="20"                        TextColor="{DynamicResource LabelTextColor}">                            <Label.GestureRecognizers>                                <TapGestureRecognizer Tapped="submit_click" />                            </Label.GestureRecognizers>                        </Label>                    </Frame>                                  </StackLayout>                  <StackLayout  VerticalOptions="End">                    <Frame                    Margin="0,0,0,0"                    BackgroundColor="Chocolate"                    Style="{DynamicResource ButtonFrameStyle}">                        <!--<Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped" />                        </Frame.GestureRecognizers>-->                        <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Logout"                        FontSize="20"                        TextColor="{DynamicResource LabelTextColor}">                            <Label.GestureRecognizers>                                <TapGestureRecognizer Tapped="Log_outclick" />                            </Label.GestureRecognizers>                        </Label>                    </Frame>                                  </StackLayout>                              </StackLayout>        </ScrollView>    </ContentPage.Content></base:BasePage>