<?xml version="1.0" encoding="utf-8" ?><TabbedPage    x:Class="DellyShopApp.Views.Pages.HomeTabbedPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:xct="http://xamarin.com/schemas/2020/toolkit"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:plugin="clr-namespace:Plugin.Badge.Abstractions;assembly=Plugin.Badge.Abstractions"    xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core"    xmlns:tabbedPages="clr-namespace:DellyShopApp.Views.TabbedPages;assembly=DellyShopApp"    xmlns:tabbedPages1="clr-namespace:DellyShopApp.Views.Pages"    android:TabbedPage.IsSwipePagingEnabled="True"    android:TabbedPage.ToolbarPlacement="Bottom"    BarBackgroundColor="{DynamicResource ItemColor}"    BarTextColor="Transparent"    NavigationPage.HasNavigationBar="False"    SelectedTabColor="#00c58a"    UnselectedTabColor="{DynamicResource WhiteDarkFontColor}">    <!--  Pages can be added as references or inline  -->   <tabbedPages:HomePage IconImageSource="Home"/>    <tabbedPages:MyCartPage    IconImageSource="Basket" x:Name="basket"    plugin:TabBadge.BadgePosition="PositionTopRight"     plugin:TabBadge.BadgeText="99+"    plugin:TabBadge.BadgeFont="20"     plugin:TabBadge.BadgeTextColor="Black"    plugin:TabBadge.BadgeColor="#00c58a"/>    <tabbedPages:ProfilePage IconImageSource="Profile"/>    </TabbedPage>