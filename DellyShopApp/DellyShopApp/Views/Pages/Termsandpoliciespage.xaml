<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.TabbedPages.Termsandpoliciespage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:forms="clr-namespace:FFImageLoading.Forms;assembly=FFImageLoading.Forms"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"     xmlns:lottie="clr-namespace:Lottie.Forms;assembly=Lottie.Forms"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView" xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5">    <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">        <StackLayout Margin="10">                        <Grid HorizontalOptions="FillAndExpand" Margin="0,10,0,10">            <Grid.RowDefinitions>                <RowDefinition Height="*" />            </Grid.RowDefinitions>            <Grid.ColumnDefinitions>                <ColumnDefinition Width="50" />                <ColumnDefinition Width="*" />                <ColumnDefinition Width="50" />            </Grid.ColumnDefinitions>            <Image            x:Name="BackButton"            Grid.Row="0"            Grid.Column="0"            Aspect="AspectFill"            HeightRequest="50"            HorizontalOptions="Start"            VerticalOptions="CenterAndExpand"            WidthRequest="50">                <Image.GestureRecognizers>                    <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_3"></TapGestureRecognizer>                </Image.GestureRecognizers>                <Image.Source>                    <FontImageSource                    FontFamily="{DynamicResource MaterialFontFamily}"                    Glyph="{x:Static font:FontAwesomeIcons.ThinLeft}"                    Size="50"                    Color="Chocolate" />                    </Image.Source>            </Image>            <Label            x:Name="Title"            Grid.Row="0"            Grid.Column="1"            Margin="0,0,0,0"            FontFamily="{DynamicResource VerdanaProRegular}"            FontSize="22"            FontAttributes="Bold"            HorizontalOptions="CenterAndExpand"            Style="{StaticResource LabelStyle}"            LineBreakMode="TailTruncation"            Text="Terms And Policies"            TextColor="Chocolate"            VerticalOptions="CenterAndExpand"            VerticalTextAlignment="Center" />                  </Grid>                <StackLayout x:Name="MainStack">                <pancakeView:PancakeView HorizontalOptions="FillAndExpand"                                          Margin="0,50,0,0"                                         ClassId="EditProfile"                                         BackgroundGradientAngle="100"                                          BackgroundGradientEndColor="White"                                         BackgroundGradientStartColor="White"                                         HasShadow="True"                                         x:Name="EditProfile"                                         BackgroundColor="#dcd6d0"                                         Style="{DynamicResource PancakeFrameStyle}">                    <pancakeView:PancakeView.GestureRecognizers>                        <TapGestureRecognizer Tapped="TermsandConditionsclick" />                    </pancakeView:PancakeView.GestureRecognizers>                    <StackLayout Orientation="Horizontal" >                                                  <Label                            FontSize="20"                             Style="{DynamicResource ProfleLabelStyle}"                            Text="Terms and Conditions" />                        <Image                            Aspect="AspectFit"                            HorizontalOptions="EndAndExpand"                            Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                    Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView                    Margin="0,10,0,0"                    ClassId="LastView"                    BackgroundGradientAngle="100"                     BackgroundGradientEndColor="White"                    BackgroundGradientStartColor="White"                    HasShadow="True"                    HorizontalOptions="FillAndExpand"                     BackgroundColor="#dcd6d0"                    x:Name="LastView"                    Style="{DynamicResource PancakeFrameStyle}">                    <pancakeView:PancakeView.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContactUsclick" />                    </pancakeView:PancakeView.GestureRecognizers>                    <StackLayout Orientation="Horizontal">                                               <Label                            FontSize="20"                             Style="{DynamicResource ProfleLabelStyle}"                            Text="Contact Us" />                        <Image                            Aspect="AspectFit"                            HorizontalOptions="EndAndExpand"                            Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                    Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView                    Margin="0,10,0,0"                    ClassId="MyFav"                    BackgroundGradientAngle="100"                     BackgroundGradientEndColor="White"                    BackgroundGradientStartColor="White"                    HasShadow="True"                    BackgroundColor="#dcd6d0"                    x:Name="MyFav"                    HorizontalOptions="FillAndExpand"                    Style="{DynamicResource PancakeFrameStyle}">                    <pancakeView:PancakeView.GestureRecognizers>                        <TapGestureRecognizer Tapped="CancellationandRefundPolicyclick" />                    </pancakeView:PancakeView.GestureRecognizers>                    <StackLayout Orientation="Horizontal">                                               <Label                            FontSize="20"                            Style="{DynamicResource ProfleLabelStyle}"                            Text="Cancellation and Refund Policy" />                        <Image                            Aspect="AspectFit"                            HorizontalOptions="EndAndExpand"                            Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                    Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView                    Margin="0,10,0,0"                    ClassId="MyOder"                      BackgroundGradientAngle="100"                                              BackgroundGradientEndColor="White"                                          BackgroundGradientStartColor="White"                                                HasShadow="True"                    x:Name="MyOder"                     BackgroundColor="#dcd6d0"                    HorizontalOptions="FillAndExpand"                    Style="{DynamicResource PancakeFrameStyle}">                    <pancakeView:PancakeView.GestureRecognizers>                        <TapGestureRecognizer Tapped="PrivacyPolicyclick" />                    </pancakeView:PancakeView.GestureRecognizers>                    <StackLayout Orientation="Horizontal">                                                <Label                            FontSize="20"                            Style="{DynamicResource ProfleLabelStyle}"                            Text="Privacy Policy" />                        <Image                            Aspect="AspectFit"                            HorizontalOptions="EndAndExpand"                            Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                    Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView                    Margin="0,10,0,0"                    ClassId="Policy"                      BackgroundGradientAngle="100"                                              BackgroundGradientEndColor="White"                                          BackgroundGradientStartColor="White"                                                HasShadow="True"                    x:Name="Policy"                     BackgroundColor="#dcd6d0"                    HorizontalOptions="FillAndExpand"                    Style="{DynamicResource PancakeFrameStyle}">                    <pancakeView:PancakeView.GestureRecognizers>                        <TapGestureRecognizer Tapped="ShippingandDeliveryPolicyclick" />                    </pancakeView:PancakeView.GestureRecognizers>                    <StackLayout Orientation="Horizontal">                                               <Label                            FontSize="20"                            Style="{DynamicResource ProfleLabelStyle}"                            Text="Shipping and Delivery Policy" />                        <Image                            Aspect="AspectFit"                            HorizontalOptions="EndAndExpand"                            Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.ThinRight}"                                    Color="Black" />                            </Image.Source>                        </Image>                    </StackLayout>                </pancakeView:PancakeView>                                                                                          </StackLayout>        </StackLayout>    </ScrollView></base:BasePage>