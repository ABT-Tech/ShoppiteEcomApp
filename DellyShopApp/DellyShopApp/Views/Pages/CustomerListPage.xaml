<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.CustomerListPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"     xmlns:renderers="clr-namespace:DellyShopApp.Renderers"    iOsSpecific:Page.UseSafeArea="True"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">           <StackLayout Margin="10">            <StackLayout Margin="15">                <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Customer List" />            </StackLayout>            <pancakeView:PancakeView HeightRequest="50"                                    Style="{DynamicResource PancakeFrameStyle}"                                     CornerRadius="10">                                    <StackLayout Orientation="Horizontal">                        <Label Text="Users List"                       HorizontalOptions="Start" VerticalOptions="Center"                       Margin="10" WidthRequest="200"                       FontSize="20"                        FontAttributes="Bold"/>                <Label Text="Active :-"                       HorizontalOptions="EndAndExpand"                       VerticalOptions="Center"                       FontSize="16"/>                <CheckBox IsVisible="true"                          HorizontalOptions="Center"                          IsChecked="True"                                                    Color="#ff002c"                           x:Name="Check"                           CheckedChanged="Check_CheckedChanged" />                </StackLayout>             </pancakeView:PancakeView>            <SearchBar Placeholder="Search items..."
           CancelButtonColor="Gray"
           PlaceholderColor="Black"
           TextColor="Black"
           TextTransform="Lowercase"
           FontSize="Medium"
           TextChanged="SearchBar_TextChanged" />
            <ListView x:Name="searchResults" IsVisible="False" ItemSelected="searchResults_ItemSelected" BackgroundColor="White">
                <ListView.ItemTemplate >
                    <DataTemplate>
                    <TextCell Text="{Binding Username}">
                        </TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>                    <ScrollView>            <customControl:RepeaterView                x:Name="BasketItems"                Margin="0,0,0,10"                Padding="2"                ItemsSource="{Binding ProcutListModel}">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell>                            <SwipeView>                                <SwipeView.RightItems>                                    <SwipeItems>                                        <SwipeItemView >                                            <Frame                                                         Margin="0,-5,0,-5"                                                        Padding="5"                                                        BackgroundColor="#FB5F5F"                                                        CornerRadius="10"                                                        HasShadow="False"                                                        IsVisible="true"                                                        WidthRequest="60">                                                <Image                                                            Aspect="AspectFit"                                                            HeightRequest="30"                                                            HorizontalOptions="CenterAndExpand"                                                            VerticalOptions="CenterAndExpand"                                                            WidthRequest="40">                                                    <Image.GestureRecognizers>                                                        <TapGestureRecognizer Tapped="Blacklisted"></TapGestureRecognizer>                                                    </Image.GestureRecognizers>                                                    <Image.Source>                                                        <FontImageSource                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                                    Glyph="{x:Static font:FontAwesomeIcons.UiBlock}"                                                                    Size="30"                                                                    Color="White" />                                                    </Image.Source>                                                </Image>                                            </Frame>                                        </SwipeItemView>                                    </SwipeItems>                                </SwipeView.RightItems>                                <SwipeView.LeftItems>                                    <SwipeItems>                                        <SwipeItemView >                                            <Frame                                                         Margin="0,-5,0,-5"                                                        Padding="5"                                                        BackgroundColor="#98D8AA"                                                        CornerRadius="10"                                                        HasShadow="False"                                                        IsVisible="true"                                                        WidthRequest="60">                                                <Image                                                            Aspect="AspectFit"                                                            HeightRequest="30"                                                            HorizontalOptions="CenterAndExpand"                                                            VerticalOptions="CenterAndExpand"                                                            WidthRequest="40">                                                    <Image.GestureRecognizers>                                                        <TapGestureRecognizer Tapped="Whitelisted"></TapGestureRecognizer>                                                    </Image.GestureRecognizers>                                                    <Image.Source>                                                        <FontImageSource                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                                    Glyph="{x:Static font:FontAwesomeIcons.Checked}"                                                                    Size="50"                                                                    Color="White" />                                                    </Image.Source>                                                </Image>                                            </Frame>                                        </SwipeItemView>                                    </SwipeItems>                                </SwipeView.LeftItems>                                <StackLayout>                                    <pancakeView:PancakeView                                Margin="0,10,0,0"                                HeightRequest="70"                                Style="{DynamicResource PancakeFrameStyle}">                                        <StackLayout>                                            <StackLayout                                         BackgroundColor="{DynamicResource ItemColor}"                                        Orientation="Horizontal"                                        VerticalOptions="FillAndExpand">                                                <pancakeView:PancakeView Padding="0"                                                              HorizontalOptions="Start"                                                              VerticalOptions="FillAndExpand"                                                              Style="{DynamicResource PancakeFrameStyle}">                                                </pancakeView:PancakeView>                                                <StackLayout HorizontalOptions="Start"  VerticalOptions="CenterAndExpand" >                                                    <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="18"                                            HorizontalOptions="StartAndExpand"                                            Text="{Binding Username}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                                    <Label                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="15"                                            HorizontalOptions="Start"                                            Text="{Binding Email}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                                </StackLayout>                                                <StackLayout VerticalOptions="Center" HorizontalOptions="EndAndExpand" Margin="0,0,10,0">                                                    <pancakeView:PancakeView Padding="0"                                                              HorizontalOptions="EndAndExpand"                                                              VerticalOptions="Center"                                                              Style="{DynamicResource PancakeFrameStyle}">                                                        <Image                                            Aspect="AspectFill"                                             HeightRequest="50"                                            Source="{Binding icon}"                                            WidthRequest="50" />                                                    </pancakeView:PancakeView>                                                </StackLayout>                                            </StackLayout>                                        </StackLayout>                                    </pancakeView:PancakeView>                                </StackLayout>                            </SwipeView>                        </ViewCell>                    </DataTemplate>                </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>        </ScrollView>    </StackLayout>    </base:BasePage>  