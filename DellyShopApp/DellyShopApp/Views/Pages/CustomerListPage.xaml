<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.Pages.CustomerListPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:languages="clr-namespace:DellyShopApp.Languages;assembly=DellyShopApp"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"     xmlns:renderers="clr-namespace:DellyShopApp.Renderers"    iOsSpecific:Page.UseSafeArea="True"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False">           <StackLayout Margin="10">            <StackLayout Margin="15">                <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Customer List" />            </StackLayout>            <pancakeView:PancakeView                                     Style="{DynamicResource PancakeFrameStyle}"                                     CornerRadius="10">                <StackLayout   Orientation="Horizontal"                                            VerticalOptions="FillAndExpand" HorizontalOptions="StartAndExpand" HeightRequest="150">                    <StackLayout WidthRequest="200" >                        <Label Text="Users"                       HorizontalOptions="Start"                       Margin="10"                       FontSize="20"                        FontAttributes="Bold"/>                    </StackLayout>                    <StackLayout Orientation="Vertical" HorizontalOptions="End" VerticalOptions="Center" >                        <Label Text="Active : "                           HorizontalOptions="End"                           FontSize="15"/>                        <Label Text="InActive : "                           HorizontalOptions="End"                           FontSize="15"/>                    </StackLayout>                    <StackLayout Orientation="Vertical" HorizontalOptions="End" VerticalOptions="Center" >                        <Image                           Aspect="AspectFill"                            HeightRequest="17"                           Source="Green.jpeg"                           WidthRequest="17" >                            <Image.GestureRecognizers>                                <TapGestureRecognizer Tapped="Greenimg"/>                            </Image.GestureRecognizers>                        </Image>                        <Image                           Aspect="AspectFill"                            HeightRequest="17"                           Source="Red1.jpeg"                           WidthRequest="17" >
                            <Image.GestureRecognizers>                                <TapGestureRecognizer Tapped="Redimg"/>                            </Image.GestureRecognizers>
                        </Image>                                    </StackLayout>                </StackLayout>            </pancakeView:PancakeView>            <SearchBar Placeholder="Search items..."
           CancelButtonColor="Gray"
           PlaceholderColor="Black"
           TextColor="Black"
           TextTransform="Lowercase"
           FontSize="Medium"
           TextChanged="SearchBar_TextChanged" />
            <ListView x:Name="searchResults" IsVisible="False" ItemSelected="searchResults_ItemSelected">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding Title}">
                        </TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>        <ScrollView>            <customControl:RepeaterView                x:Name="BasketItems"                Margin="0,0,0,10"                Padding="2"                ItemsSource="{Binding ProcutListModel}">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell>                            <StackLayout>                                <pancakeView:PancakeView                                Margin="0,10,0,0"                                HeightRequest="70"                                Style="{DynamicResource PancakeFrameStyle}">                                    <StackLayout>                                        <StackLayout                                         BackgroundColor="{DynamicResource ItemColor}"                                        Orientation="Horizontal"                                        VerticalOptions="FillAndExpand">                                            <pancakeView:PancakeView Padding="0"                                                              HorizontalOptions="Start"                                                              VerticalOptions="FillAndExpand"                                                              Style="{DynamicResource PancakeFrameStyle}">                                            </pancakeView:PancakeView>                                            <StackLayout  VerticalOptions="CenterAndExpand" WidthRequest="250">                                                <Label                                            FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="18"                                            HorizontalOptions="StartAndExpand"                                            Text="{Binding Username}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                                <Label                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="15"                                            HorizontalOptions="Start"                                            Text="{Binding Email}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                            </StackLayout>                                            <StackLayout VerticalOptions="Center" HorizontalOptions="End">                                                <pancakeView:PancakeView Padding="0"                                                              HorizontalOptions="End"                                                              VerticalOptions="Center"                                                              Style="{DynamicResource PancakeFrameStyle}">                                                    <Image                                            Aspect="AspectFill"                                             HeightRequest="50"                                            Source="{Binding icon}"                                            WidthRequest="50" />                                                </pancakeView:PancakeView>                                            </StackLayout>                                        </StackLayout>                                    </StackLayout>                                </pancakeView:PancakeView>                            </StackLayout>                        </ViewCell>                    </DataTemplate>                </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>        </ScrollView>    </StackLayout>    </base:BasePage>  