<?xml version="1.0" encoding="utf-8" ?><base:BasePage    x:Class="DellyShopApp.Views.TabbedPages.MyCartPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:iOsSpecific="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews;assembly=DellyShopApp"    xmlns:xct="http://xamarin.com/schemas/2020/toolkit"    iOsSpecific:Page.UseSafeArea="True"    xmlns:languages="clr-namespace:DellyShopApp.Languages"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Material">    <ScrollView>        <StackLayout Margin="10">                     <Grid HorizontalOptions="FillAndExpand" Margin="0,10,0,10">                <Grid.RowDefinitions>                    <RowDefinition Height="*" />                </Grid.RowDefinitions>                <Grid.ColumnDefinitions>                    <ColumnDefinition Width="50" />                    <ColumnDefinition Width="*" />                    <ColumnDefinition Width="50" />                </Grid.ColumnDefinitions>                <Image            x:Name="BackButton"            Grid.Row="0"            Grid.Column="0"            Aspect="AspectFill"            HeightRequest="50"            HorizontalOptions="Start"            VerticalOptions="CenterAndExpand"            WidthRequest="50">                    <Image.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_4"></TapGestureRecognizer>                    </Image.GestureRecognizers>                    <Image.Source>                        <FontImageSource                    FontFamily="{DynamicResource MaterialFontFamily}"                    Glyph="{x:Static font:FontAwesomeIcons.ThinLeft}"                    Size="50"                    Color="#ff002c" />                    </Image.Source>                </Image>                <Label            x:Name="Title"            Grid.Row="0"            Grid.Column="1"            Margin="0,0,0,0"            FontFamily="{DynamicResource VerdanaProRegular}"            FontSize="22"                       HorizontalOptions="CenterAndExpand"                            Style="{StaticResource LabelStyle}"            LineBreakMode="TailTruncation"            Text="My Cart"            TextColor="#ff002c"            VerticalOptions="CenterAndExpand"            VerticalTextAlignment="Center" />            </Grid>            <customControl:RepeaterView x:Name="BasketItems">                <customControl:RepeaterView.ItemTemplate>                    <DataTemplate>                        <ViewCell >                            <pancakeView:PancakeView Margin="0,5,0,0"  HeightRequest="100" Style="{DynamicResource PancakeFrameStyle}" >                                <pancakeView:PancakeView.GestureRecognizers>                                    <TapGestureRecognizer Tapped="ClickItem" />                                </pancakeView:PancakeView.GestureRecognizers>                                <SwipeView>                                    <SwipeView.RightItems>                                        <SwipeItems>                                            <SwipeItemView >                                                <Frame                                                         Margin="0,-5,0,-5"                                                        Padding="5"                                                        BackgroundColor="#FB5F5F"                                                        CornerRadius="10"                                                        HasShadow="False"                                                        IsVisible="true"                                                        WidthRequest="60">                                                    <Image                                                            Aspect="AspectFit"                                                            HeightRequest="30"                                                            HorizontalOptions="CenterAndExpand"                                                            VerticalOptions="CenterAndExpand"                                                            WidthRequest="30">                                                        <Image.GestureRecognizers>                                                            <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_1"></TapGestureRecognizer>                                                        </Image.GestureRecognizers>                                                        <Image.Source>                                                            <FontImageSource                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                                    Glyph="{x:Static font:FontAwesomeIcons.Close}"                                                                    Size="30"                                                                    Color="White" />                                                        </Image.Source>                                                    </Image>                                                </Frame>                                            </SwipeItemView>                                        </SwipeItems>                                    </SwipeView.RightItems>                                    <StackLayout                                        BackgroundColor="White"                                        Orientation="Horizontal"                                        VerticalOptions="Center">                                        <pancakeView:PancakeView                                            Padding="0"                                            HorizontalOptions="End"                                            Style="{DynamicResource PancakeFrameStyle}"                                             VerticalOptions="FillAndExpand">                                            <Image                                         Aspect="AspectFit"                                        HeightRequest="100"                                        Source="{Binding Image}"                                        WidthRequest="100" />                                        </pancakeView:PancakeView>                                                    <StackLayout  WidthRequest="200">                                    <Label VerticalOptions="StartAndExpand"                                           HorizontalOptions="FillAndExpand"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="18"                                            LineBreakMode="TailTruncation"                                            Text="{Binding Title}"                                            MaxLines="1"                                            TextColor="{DynamicResource WhiteDarkFontColor}"/>                                    <Label VerticalOptions="CenterAndExpand"                                           HorizontalOptions="FillAndExpand"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                            FontSize="14"                                            Text="{Binding Brand}"                                            TextColor="Gray" />                                                                                            <Label Margin="0,0,10,0"                                             FontAttributes="Bold" x:Name="price"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                             FontSize="18"                                             HorizontalOptions="FillAndExpand"                                             Text="{Binding Price, StringFormat='{0} ₹'}"                                             TextColor="Black"                                             MaxLines="1"                                           IsVisible="{Binding IsPriceVisible}"                                          VerticalOptions="EndAndExpand" />                                             <Label Margin="0,0,10,0"                                             FontAttributes="Bold"                                             FontFamily="{DynamicResource VerdanaProRegular}"                                             FontSize="18"                                             HorizontalOptions="FillAndExpand"                                                   Text="Out Of Stock!"                                             TextColor="Red" IsVisible="false"                                          VerticalOptions="EndAndExpand" />                                        </StackLayout>                                        <StackLayout Orientation="Horizontal" HorizontalOptions="End" IsVisible="{Binding IsPriceVisible}">                                                        <Image  Margin="{x:OnIdiom Tablet='0,0,10,0',                                               Phone='0,0,10,0'}" x:Name="minusimg"                                                Aspect="AspectFit"                                                HeightRequest="20"                                                HorizontalOptions="Start"                                                VerticalOptions="CenterAndExpand"                                                 WidthRequest="20">                                                            <Image.GestureRecognizers>                                                                <TapGestureRecognizer Tapped="MinusClick" />                                                            </Image.GestureRecognizers>                                                            <Image.Source>                                                                <FontImageSource                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                    Glyph="{x:Static font:FontAwesomeIcons.Minus}"                                                    Size="10"                                                    Color="#ff002c" />                                                            </Image.Source>                                                        </Image>                                                        <Label WidthRequest="35" x:Name="countlbl"                                            FontFamily="{DynamicResource VerdanaProLight}"                                            FontSize="25"                                            HorizontalOptions="Center"                                            Text="{Binding Quantity}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                            VerticalOptions="CenterAndExpand" />                                                        <Label WidthRequest="35"                                            FontFamily="{DynamicResource VerdanaProLight}"                                             FontSize="25"                                                 HorizontalOptions="Center"                                                Text="{Binding Id}"                                            TextColor="{DynamicResource WhiteDarkFontColor}"                                                IsVisible="False"/>                                                        <Image x:Name="plusimg"                                     Margin="{x:OnIdiom Tablet='0,0,10,0',                                               Phone='0,0,10,0'}"                                            Aspect="AspectFit"                                            HeightRequest="20"                                             HorizontalOptions="End"                                            VerticalOptions="CenterAndExpand"                                             WidthRequest="20">                                                            <Image.GestureRecognizers>                                                                <TapGestureRecognizer Tapped="PlusClick" />                                                            </Image.GestureRecognizers>                                                            <Image.Source>                                                                <FontImageSource                                                FontFamily="{DynamicResource MaterialFontFamily}"                                                Glyph="{x:Static font:FontAwesomeIcons.Plus}"                                                Size="10"                                                Color="#ff002c" />                                                            </Image.Source>                                                        </Image>                                                    </StackLayout>                                                </StackLayout>                                </SwipeView>                            </pancakeView:PancakeView>                        </ViewCell>                    </DataTemplate>                </customControl:RepeaterView.ItemTemplate>            </customControl:RepeaterView>            <Image Source="asd.gif" HeightRequest="350" WidthRequest="500"                   HorizontalOptions="Center"                   VerticalOptions="Start"                   x:Name="gif" ></Image>             <Image Source="YourCartisempty.gif" HeightRequest="350" WidthRequest="500"                   HorizontalOptions="Center"                   VerticalOptions="CenterAndExpand"                   x:Name="cartimg" ></Image>                       <StackLayout x:Name="shopping" VerticalOptions="EndAndExpand">                <Frame                     BackgroundColor="#ff002c"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="TapGestureRecognizer_Tapped_2" />                    </Frame.GestureRecognizers>                    <Image                            Grid.Column="0"                            Aspect="AspectFit"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                        <Image.Source>                            <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Shop Now"                                    Size="20"                                    Color="White" />                        </Image.Source>                    </Image>                </Frame>            </StackLayout>             <StackLayout x:Name="checkout" VerticalOptions="EndAndExpand">                <Frame                    Margin="30"                    BackgroundColor="#ff002c"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="Button_Clicked" />                    </Frame.GestureRecognizers>                    <Image                            Grid.Column="0"                            Aspect="AspectFit"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                        <Image.Source>                            <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Process To CheckOut"                                    Size="20"                                    Color="White" />                        </Image.Source>                    </Image>                </Frame>            </StackLayout>             <StackLayout  VerticalOptions="EndAndExpand"  x:Name="Login">                <Frame                    Margin="20,10,20,10"                    BackgroundColor="#ff002c"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="LogInClick" />                    </Frame.GestureRecognizers>                    <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Login" FontAttributes="Bold"                        FontSize="20"  xct:TouchEffect.AnimationDuration="250"                                                               xct:TouchEffect.PressedScale="0.75"                                                               xct:TouchEffect.PressedOpacity="0.6"                        TextColor="White">                        <Label.GestureRecognizers>                            <TapGestureRecognizer Tapped="LogInClick" />                        </Label.GestureRecognizers>                    </Label>                </Frame>            </StackLayout>        </StackLayout>    </ScrollView></base:BasePage> 