<?xml version="1.0" encoding="utf-8" ?><base:BasePage     x:Class="DellyShopApp.Views.TabbedPages.BasketPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:helpers="clr-namespace:DellyShopApp.Languages"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Default">       <ContentPage.Content>        <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">                <StackLayout                x:Name="MainStack"                Margin="10"                Padding="5"                Spacing="10">                      <ActivityIndicator x:Name="uploadIndicator" IsVisible="False" IsRunning="False" Color="Chocolate" />                <StackLayout                    Margin="0,10,0,0"                    HeightRequest="50"                    VerticalOptions="Start">                      <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Procced To CheckOut"/>                  </StackLayout>                <customControl:RepeaterView x:Name="BasketItems">                    <customControl:RepeaterView.ItemTemplate>                        <DataTemplate>                            <ViewCell>                                <pancakeView:PancakeView                                    Margin="0,10,0,0"                                    Padding="5"                                    HeightRequest="70"                                    Style="{DynamicResource PancakeFrameStyle}">                                    <pancakeView:PancakeView.GestureRecognizers>                                        <TapGestureRecognizer Tapped="ClickItem" />                                    </pancakeView:PancakeView.GestureRecognizers>                                                                          <!--  Content  -->                                        <StackLayout                                            BackgroundColor="{DynamicResource ItemColor}"                                            Orientation="Horizontal"                                            VerticalOptions="FillAndExpand">                                            <pancakeView:PancakeView                                                Padding="0"                                                HorizontalOptions="End"                                                Style="{DynamicResource PancakeFrameStyle}"                                                VerticalOptions="FillAndExpand">                                                <Image                                                    Aspect="AspectFit"                                                    HeightRequest="60"                                                    Source="{Binding Image}"                                                    WidthRequest="70" />                                            </pancakeView:PancakeView>                                            <StackLayout  HorizontalOptions="StartAndExpand" VerticalOptions="Center" Margin="10,0,10,0">                                                <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="15"                                                    MaxLines="1"                                                    LineBreakMode="TailTruncation"                                                    Text="{Binding Title}"                                                    TextColor="{DynamicResource WhiteDarkFontColor}" />                                                <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="13"                                                    Text="{Binding Brand}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                             <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="13"                                                                                                       Text="{Binding SpecificationNames}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                            </StackLayout>                                        <StackLayout HorizontalOptions="EndAndExpand" MinimumWidthRequest="140" Orientation="Horizontal" VerticalOptions="Center">                                            <Label                                                 Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="18"                                                HorizontalOptions="End"                                                Text="{Binding Price, StringFormat='₹{0}'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                              <Label                                                Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="14"                                                HorizontalOptions="End"                                                Text="{Binding Quantity, StringFormat='X {0}'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                        </StackLayout>                                </pancakeView:PancakeView>                            </ViewCell>                        </DataTemplate>                    </customControl:RepeaterView.ItemTemplate>                </customControl:RepeaterView>                  <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}" HeightRequest="65">                    <StackLayout Margin="10" VerticalOptions="CenterAndExpand" Orientation="Horizontal">                        <Frame Style="{DynamicResource FrameStyle}"                               BackgroundColor="Transparent"                               x:Name="alert"                               Padding="2" HeightRequest="60"                                CornerRadius="10"                                BorderColor="Gray">                            <renderers:BorderlessEntry x:Name="CouponCode"                                                        FontSize="15"                                                        Margin="5,0,5,0" ClearButtonVisibility="WhileEditing"                                                Placeholder="Enter Coupon Code" PlaceholderColor="Gray"                                                         TextColor="Red" FontAttributes="Bold"                                                       TextTransform="Uppercase"                                                 Style="{DynamicResource BorderlessEditor}" />                        </Frame>                       <Frame                    BackgroundColor="#E59500" HorizontalOptions="EndAndExpand"                    Style="{DynamicResource ButtonFrameStyle}">                            <Frame.GestureRecognizers>                                <TapGestureRecognizer Tapped="Button_Clicked_1" />                            </Frame.GestureRecognizers>                            <Image Margin="15,5,15,5"                            Aspect="AspectFill"                                HorizontalOptions="CenterAndExpand"                                                       VerticalOptions="CenterAndExpand">                                <Image.Source>                                    <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Apply"                                    Size="18"                                    Color="White" />                                </Image.Source>                            </Image>                        </Frame>                    </StackLayout>                </pancakeView:PancakeView>                <Label x:Name="lbl_CouponId" IsVisible="false"></Label>                <Label x:Name="lbl_IsCouponApplied" IsVisible="false"></Label>                <BoxView                    Margin="10"                    BackgroundColor="DarkGray"                    HeightRequest=".5"                    HorizontalOptions="FillAndExpand"                    VerticalOptions="Center" />                 <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.TruckAlt}"                                    Size="50"                                    Color="{DynamicResource IconBgColor}" />                            </Image.Source>                        </Image>                        <StackLayout Spacing="0" VerticalOptions="CenterAndExpand">                            <Label                                FontFamily="{DynamicResource VerdanaProRegular}"                                FontSize="15"                                HorizontalOptions="StartAndExpand"                                Text="Free Shipping"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                VerticalOptions="Center" />                            <!--<Label                                FontAttributes="Bold"                                FontFamily="{DynamicResource VerdanaProLight}"                                FontSize="10"                                HorizontalOptions="StartAndExpand"                                Text="Express"                                TextColor="Black"                                VerticalOptions="Center" />-->                        </StackLayout>                        <Label                            Margin="0,0,10,0"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            Text="₹0"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                  <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.Gift}"                                    Size="50"                                    Color="{DynamicResource IconBgColor}" />                            </Image.Source>                        </Image>                                                  <Label                                FontFamily="{DynamicResource VerdanaProRegular}"                                FontSize="15"                                HorizontalOptions="StartAndExpand"                                Text="Delivered In 5 To 7 Days"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                VerticalOptions="Center" />                                                 <StackLayout HorizontalOptions="End">                                <Label                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="18"                            HorizontalOptions="EndAndExpand"                            TextColor="{DynamicResource WhiteDarkFontColor}"                                Text="5 to 7 Days"                            VerticalOptions="CenterAndExpand" />                            </StackLayout>                                          </StackLayout>                </pancakeView:PancakeView>                <pancakeView:PancakeView                    Style="{DynamicResource PancakeFrameStyle}"                    x:Name="price"                    IsVisible="true">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="{x:Static helpers:AppResources.TotalPrice}"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPrice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                  <pancakeView:PancakeView                    Style="{DynamicResource PancakeFrameStyle}"                    x:Name="discont"                    IsVisible="false">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="{x:Static helpers:AppResources.TotalPrice}"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPricee"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand"                            TextDecorations="Strikethrough"/>                    </StackLayout>                </pancakeView:PancakeView>                  <pancakeView:PancakeView                    Style="{DynamicResource PancakeFrameStyle}"                    x:Name="discontprice"                    IsVisible="false">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="Disconted Price"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="Discountprice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand"                            />                    </StackLayout>                </pancakeView:PancakeView>                <StackLayout>                    <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                        <StackLayout Orientation="Horizontal">                            <StackLayout  VerticalOptions="Center" HorizontalOptions="StartAndExpand" MinimumWidthRequest="150">                                <Label Style="{DynamicResource NormalLabelStyle}"                                   VerticalOptions="Center"                                    Margin="5" x:Name="SelAdd"                                   Text="Select Address:-" LineBreakMode="TailTruncation" MaxLines="1" />                            </StackLayout>                            <StackLayout HorizontalOptions="EndAndExpand" VerticalOptions="Center" Margin="10,0,10,0"  >                                <renderers:BorderlessPicker                                x:Name="AddressPicker"                                  ItemDisplayBinding="{Binding AddressDetail}"                                Title="Select Address:-"                                Style="{DynamicResource BorderlessPicker}"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                TitleColor="{DynamicResource WhiteDarkFontColor}"/>                            </StackLayout>                        </StackLayout>                    </pancakeView:PancakeView>                </StackLayout>                <StackLayout  VerticalOptions="EndAndExpand">                 <Frame x:Name="onepay" IsVisible="false"                    Margin="0,0,0,10"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                        <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="BrowserUrl" />                        </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="1Pay"                                    Size="20"                                    Color="Black" />                            </Image.Source>                        </Image>                    </Frame>                    <!--<Frame                    Margin="0,0,0,20"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueWithPaypal" />                    </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Paytm"                                    Size="20"                                    Color="{DynamicResource LabelTextColor}" />                            </Image.Source>                        </Image>                </Frame>-->                <Frame                    Margin="0,0,0,400"                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueClick" />                    </Frame.GestureRecognizers>                    <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Cash On Delivery"                        FontSize="20"                        TextColor="Black">                        <Label.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Label.GestureRecognizers>                    </Label>                </Frame>                </StackLayout>            </StackLayout>        </ScrollView>    </ContentPage.Content></base:BasePage>