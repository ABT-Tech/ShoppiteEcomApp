<?xml version="1.0" encoding="utf-8" ?><base:BasePage     x:Class="DellyShopApp.Views.TabbedPages.BasketPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:helpers="clr-namespace:DellyShopApp.Languages"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"            xmlns:xct="http://xamarin.com/schemas/2020/toolkit"
    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Material">    <ContentPage.Content>        <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">            <StackLayout                x:Name="MainStack"                Margin="5"                Padding="5"                Spacing="10">                <StackLayout                    Margin="0,10,0,0"                    HeightRequest="50"                    VerticalOptions="Start">                    <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Procced To CheckOut"/>                </StackLayout>                <customControl:RepeaterView x:Name="BasketItems">                    <customControl:RepeaterView.ItemTemplate>                        <DataTemplate>                            <ViewCell>                                <pancakeView:PancakeView                                    Margin="0,10,0,0"                                    Padding="5"                                    Style="{DynamicResource PancakeFrameStyle}">                                    <pancakeView:PancakeView.GestureRecognizers>                                        <TapGestureRecognizer Tapped="ClickItem"/>                                    </pancakeView:PancakeView.GestureRecognizers>                                                                            <StackLayout                                            BackgroundColor="{DynamicResource ItemColor}"                                            Orientation="Horizontal"                                            VerticalOptions="FillAndExpand">                                            <pancakeView:PancakeView                                                Padding="0"                                                HorizontalOptions="Start"                                                Style="{DynamicResource PancakeFrameStyle}"                                                VerticalOptions="FillAndExpand">                                                <Image                                                    Aspect="AspectFit"                                                    HeightRequest="50"                                                    Source="{Binding Image}"                                                    WidthRequest="50" />                                            </pancakeView:PancakeView>                                            <StackLayout HorizontalOptions="StartAndExpand" VerticalOptions="Center">                                                <Label HorizontalOptions="FillAndExpand" VerticalOptions="StartAndExpand"                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="15"                                                    Text="{Binding Title}" LineBreakMode="TailTruncation"                                                    TextColor="{DynamicResource WhiteDarkFontColor}" />                                                <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding Brand}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                            </StackLayout >                                        <StackLayout WidthRequest="130" Orientation="Horizontal" Margin="10,0,5,0" VerticalOptions="CenterAndExpand" HorizontalOptions="EndAndExpand">                                            <Label                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="14"                                                HorizontalOptions="EndAndExpand"                                                Text="{Binding Price, StringFormat='{0} ₹'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                            <Label                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="14"                                                HorizontalOptions="EndAndExpand"                                                Text="{Binding Quantity, StringFormat='X {0}'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                    </StackLayout>                                    <!--</SwipeView>-->                                </pancakeView:PancakeView>                            </ViewCell>                        </DataTemplate>                    </customControl:RepeaterView.ItemTemplate>                </customControl:RepeaterView>                <BoxView                    Margin="10"                    BackgroundColor="DarkGray"                    HeightRequest=".5"                    HorizontalOptions="FillAndExpand"                    VerticalOptions="Center" />                <!--<pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="Sub Total:"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="SubTotal"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>-->                <!--<pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.TruckAlt}"                                    Size="50"                                    Color="{DynamicResource IconBgColor}" />                            </Image.Source>                        </Image>                        <StackLayout Spacing="0" VerticalOptions="CenterAndExpand">                            <Label                                FontFamily="{DynamicResource VerdanaProRegular}"                                FontSize="15"                                HorizontalOptions="StartAndExpand"                                Text="ShippingCost"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                VerticalOptions="Center" />                            <Label                                FontAttributes="Bold"                                FontFamily="{DynamicResource VerdanaProLight}"                                FontSize="10"                                HorizontalOptions="StartAndExpand"                                Text="Express"                                TextColor="Black"                                VerticalOptions="Center" />                        </StackLayout>                        <Label                            Margin="0,0,10,0"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            Text="30₹"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>-->                <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label Margin="5,0,0,0"                            FontAttributes="Bold"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="Total Price :-"                            TextColor="#ff002c"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPrice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontSize="18"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                <StackLayout>                    <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                        <StackLayout Orientation="Horizontal">                            <StackLayout  VerticalOptions="Center" HorizontalOptions="Start">                                <Label TextColor="#ff002c"                                   VerticalOptions="Center"                                    Margin="5" x:Name="SelAdd"                                   Text="Select Address :-" FontAttributes="Bold" FontSize="18" LineBreakMode="TailTruncation" MaxLines="1" />                            </StackLayout>                            <StackLayout HorizontalOptions="EndAndExpand" VerticalOptions="Center" WidthRequest="150" >                                <renderers:BorderlessPicker                                x:Name="AddressPicker"  HorizontalOptions="EndAndExpand"                                 ItemDisplayBinding="{Binding AddressDetail}"                                Title="{x:Static helpers:AppResources.SelectAddress}"                                Style="{DynamicResource BorderlessPicker}"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                TitleColor="{DynamicResource WhiteDarkFontColor}"/>                            </StackLayout>                            <Image HorizontalOptions="End">                                <Image.Source>                                    <FontImageSource                                         FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.ThinDown}"                                        Size="40"                                        Color="#ff002c" />                                </Image.Source>                                <Image.GestureRecognizers>                                    <TapGestureRecognizer                                         Tapped="AddAddressClick"/>                                </Image.GestureRecognizers>                            </Image>                        </StackLayout>                    </pancakeView:PancakeView>                </StackLayout>                <StackLayout  VerticalOptions="EndAndExpand">                    <!--<Frame                    Margin="0,0,0,20"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueWithPaypal" />                    </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Paytm"                                    Size="20"                                    Color="{DynamicResource LabelTextColor}" />                            </Image.Source>                        </Image>                </Frame>-->                    <Frame                     Margin="30"                    BackgroundColor="#ff002c"                    Style="{DynamicResource ButtonFrameStyle}">                        <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Frame.GestureRecognizers>                        <Label  xct:TouchEffect.AnimationDuration="250"                                                               xct:TouchEffect.PressedScale="0.75"                                                               xct:TouchEffect.PressedOpacity="0.6"                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Cash On Delivery"                        FontSize="20"                         TextColor="White">                            <Label.GestureRecognizers>                                <TapGestureRecognizer Tapped="ContinueClick" />                            </Label.GestureRecognizers>                        </Label>                    </Frame>                </StackLayout>            </StackLayout>        </ScrollView>    </ContentPage.Content></base:BasePage>