<?xml version="1.0" encoding="utf-8" ?><base:BasePage     x:Class="DellyShopApp.Views.TabbedPages.BasketPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:helpers="clr-namespace:DellyShopApp.Languages"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Material">    <ContentPage.Content>        <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">                        <StackLayout                                x:Name="MainStack"                Margin="10"                Padding="5"                Spacing="10">                <ActivityIndicator x:Name="uploadIndicator" IsVisible="False" IsRunning="False"  Color="Chocolate" />
                <StackLayout                    Margin="0,10,0,0"                    HeightRequest="50"                    VerticalOptions="Start">                    <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Order Summary"/>                </StackLayout>                <customControl:RepeaterView x:Name="BasketItems">                    <customControl:RepeaterView.ItemTemplate>                        <DataTemplate>                            <ViewCell>                                <pancakeView:PancakeView                                    Margin="0,10,0,0"                                    Padding="5"  HeightRequest="70"                                    Style="{DynamicResource PancakeFrameStyle}">                                    <pancakeView:PancakeView.GestureRecognizers>                                        <TapGestureRecognizer Tapped="ClickItem" />                                    </pancakeView:PancakeView.GestureRecognizers>                                                                       <!--  Content  -->                                    <StackLayout                                            BackgroundColor="{DynamicResource ItemColor}"                                            Orientation="Horizontal"                                            VerticalOptions="FillAndExpand">                                        <pancakeView:PancakeView                                                Padding="0"                                                HorizontalOptions="Start"                                                Style="{DynamicResource PancakeFrameStyle}"                                                VerticalOptions="FillAndExpand">                                            <Image                                                    Aspect="AspectFill"                                                    HeightRequest="30"                                                    Source="{Binding Image}"                                                    WidthRequest="50" />                                        </pancakeView:PancakeView>                                        <StackLayout HorizontalOptions="StartAndExpand" VerticalOptions="Center" Margin="10,0,10,0">                                            <Label                                                     FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="15"                                                    Text="{Binding Title}" LineBreakMode="TailTruncation"                                                    TextColor="{DynamicResource WhiteDarkFontColor}" />                                            <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding Brand}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                            <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding SpecificationNames}"                                                                                                       TextColor="{DynamicResource DescLabelColor}" />                                        </StackLayout>                                        <StackLayout HorizontalOptions="EndAndExpand" MinimumWidthRequest="100" Orientation="Horizontal" VerticalOptions="Center">                                            <Label                                                Margin="0,0,2,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="14"                                                HorizontalOptions="End"                                                Text="{Binding Price, StringFormat='{0} ₹'}"                                                TextColor="Black" />                                            <Label                                                Margin="0,0,5,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="12"                                                HorizontalOptions="End"                                                Text="{Binding Quantity, StringFormat='X {0}'}"                                                TextColor="Black"/>                                        </StackLayout>                                    </StackLayout>                                    <!--</SwipeView>-->                                </pancakeView:PancakeView>                            </ViewCell>                        </DataTemplate>                    </customControl:RepeaterView.ItemTemplate>                </customControl:RepeaterView>                <BoxView                    Margin="10"                    BackgroundColor="DarkGray"                    HeightRequest=".5"                    HorizontalOptions="FillAndExpand"                    VerticalOptions="Center" />                <!--<pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="Sub Total:"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="SubTotal"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>-->                <!--<pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal">                        <Image Style="{DynamicResource ImageStyleEntry}">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="{x:Static font:FontAwesomeIcons.TruckAlt}"                                    Size="50"                                    Color="{DynamicResource IconBgColor}" />                            </Image.Source>                        </Image>                        <StackLayout Spacing="0" VerticalOptions="CenterAndExpand">                            <Label                                FontFamily="{DynamicResource VerdanaProRegular}"                                FontSize="15"                                HorizontalOptions="StartAndExpand"                                Text="ShippingCost"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                VerticalOptions="Center" />                            <Label                                FontAttributes="Bold"                                FontFamily="{DynamicResource VerdanaProLight}"                                FontSize="10"                                HorizontalOptions="StartAndExpand"                                Text="Express"                                TextColor="Black"                                VerticalOptions="Center" />                        </StackLayout>                        <Label                            Margin="0,0,10,0"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            Text="30₹"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>-->                <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="{x:Static helpers:AppResources.TotalPrice}"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPrice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                <StackLayout>                    <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                        <StackLayout Orientation="Horizontal">                            <StackLayout  VerticalOptions="Center" HorizontalOptions="StartAndExpand" MinimumWidthRequest="150">                                <Label Style="{DynamicResource NormalLabelStyle}"                                   VerticalOptions="Center"                                    Margin="5" x:Name="SelAdd"                                   Text="Select Address:-" LineBreakMode="TailTruncation" MaxLines="1" />                            </StackLayout>                            <StackLayout  HorizontalOptions="EndAndExpand" VerticalOptions="Center" Margin="10,0,10,0" >                                <renderers:BorderlessPicker                                 x:Name="AddressPicker"  HorizontalOptions="EndAndExpand"                                 ItemDisplayBinding="{Binding AddressDetail}"                                Title="{x:Static helpers:AppResources.SelectAddress}"                                Style="{DynamicResource BorderlessPicker}"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                TitleColor="{DynamicResource WhiteDarkFontColor}"/>                                                                                   </StackLayout>                        </StackLayout>                    </pancakeView:PancakeView>                </StackLayout>                <StackLayout Margin="20" VerticalOptions="EndAndExpand">                    <Frame                    Margin="0,0,0,20"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="BrowserUrl" />                    </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="One Pay"                                    Size="20"                                    Color="Black" />                            </Image.Source>                        </Image>                </Frame>                    <Frame                    Margin="0,0,0,10"                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                        <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Contnue"                                    Size="20"                                    Color="Black" />                            </Image.Source>                        </Image>                    </Frame>                    <!--<Frame                                       BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                        <Frame.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Frame.GestureRecognizers>                        <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Continue"                        FontSize="20"                        TextColor="Black">                            <Label.GestureRecognizers>                                <TapGestureRecognizer Tapped="ContinueClick" />                            </Label.GestureRecognizers>                        </Label>                    </Frame>-->                </StackLayout>            </StackLayout>        </ScrollView>    </ContentPage.Content></base:BasePage>