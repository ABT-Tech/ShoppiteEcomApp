<?xml version="1.0" encoding="utf-8" ?><base:BasePage     x:Class="DellyShopApp.Views.TabbedPages.BasketPage"    xmlns="http://xamarin.com/schemas/2014/forms"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:base="clr-namespace:DellyShopApp.Views.Pages.Base;assembly=DellyShopApp"    xmlns:customControl="clr-namespace:DellyShopApp.CustomControl;assembly=DellyShopApp"    xmlns:font="clr-namespace:DellyShopApp.Helpers;assembly=DellyShopApp"    xmlns:helpers="clr-namespace:DellyShopApp.Languages"    xmlns:pancakeView="clr-namespace:DellyShopApp.Views.CustomView"    xmlns:renderers="clr-namespace:DellyShopApp.Renderers;assembly=DellyShopApp"    xmlns:partialViews="clr-namespace:DellyShopApp.Views.PartialViews"    BackgroundColor="#f5f5f5"    NavigationPage.HasNavigationBar="False"    Visual="Material">               <ContentPage.Content>               <ScrollView HorizontalScrollBarVisibility="Never" VerticalScrollBarVisibility="Never">                                        <StackLayout                x:Name="MainStack"                Margin="10"                Padding="5"                Spacing="10">                <StackLayout                    Margin="0,10,0,0"                    HeightRequest="50"                    VerticalOptions="Start">                      <partialViews:NavigationBarPartialView            BackButtonVisibler="True"            FavVisible="False"            IsModalpage="False"            NavigationTitle="Procced To CheckOut"/>                                    </StackLayout>                <customControl:RepeaterView x:Name="BasketItems">                    <customControl:RepeaterView.ItemTemplate>                        <DataTemplate>                            <ViewCell>                                <pancakeView:PancakeView                                    Margin="0,10,0,0"                                    Padding="5"                                    Style="{DynamicResource PancakeFrameStyle}">                                    <pancakeView:PancakeView.GestureRecognizers>                                        <TapGestureRecognizer Tapped="ClickItem" />                                    </pancakeView:PancakeView.GestureRecognizers>                                    <SwipeView>                                        <SwipeView.RightItems>                                            <SwipeItems>                                                <SwipeItemView>                                                    <Frame                                                        Margin="0,-5,0,-5"                                                        Padding="5"                                                        BackgroundColor="#FB5F5F"                                                        CornerRadius="10"                                                        HasShadow="False"                                                        IsVisible="true"                                                        WidthRequest="40">                                                        <Image                                                            Aspect="AspectFit"                                                            HeightRequest="30"                                                            HorizontalOptions="CenterAndExpand"                                                            VerticalOptions="CenterAndExpand"                                                            WidthRequest="30">                                                            <Image.Source>                                                                <FontImageSource                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                                                    Glyph="{x:Static font:FontAwesomeIcons.Close}"                                                                    Size="30"                                                                    Color="White" />                                                            </Image.Source>                                                        </Image>                                                    </Frame>                                                </SwipeItemView>                                            </SwipeItems>                                        </SwipeView.RightItems>                                        <!--  Content  -->                                        <StackLayout                                            BackgroundColor="{DynamicResource ItemColor}"                                            Orientation="Horizontal"                                            VerticalOptions="FillAndExpand">                                            <pancakeView:PancakeView                                                Padding="0"                                                HorizontalOptions="Start"                                                Style="{DynamicResource PancakeFrameStyle}"                                                VerticalOptions="FillAndExpand">                                                <Image                                                    Aspect="AspectFill"                                                    HeightRequest="50"                                                    Source="{Binding Image}"                                                    WidthRequest="50" />                                            </pancakeView:PancakeView>                                            <StackLayout  HorizontalOptions="StartAndExpand" VerticalOptions="End" WidthRequest="150">                                                <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="15"                                                    MaxLines="1"                                                    LineBreakMode="TailTruncation"                                                    Text="{Binding Title}"                                                    TextColor="{DynamicResource WhiteDarkFontColor}" />                                                <Label                                                    FontFamily="{DynamicResource VerdanaProRegular}"                                                    FontSize="12"                                                    Text="{Binding Brand}"                                                    TextColor="{DynamicResource DescLabelColor}" />                                            </StackLayout>                                            <Label                                                 Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="22"                                                HorizontalOptions="End"                                                Text="{Binding Price, StringFormat='{0}₹'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                              <Label                                                Margin="0,0,10,0"                                                FontFamily="{DynamicResource VerdanaProRegular}"                                                FontSize="15"                                                HorizontalOptions="End"                                                Text="{Binding Quantity, StringFormat='X {0}'}"                                                TextColor="Black"                                                VerticalOptions="CenterAndExpand" />                                        </StackLayout>                                    </SwipeView>                                </pancakeView:PancakeView>                            </ViewCell>                        </DataTemplate>                    </customControl:RepeaterView.ItemTemplate>                </customControl:RepeaterView>                <BoxView                    Margin="10"                    BackgroundColor="DarkGray"                    HeightRequest=".5"                    HorizontalOptions="FillAndExpand"                    VerticalOptions="Center" />                                  <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                    <StackLayout Orientation="Horizontal" VerticalOptions="CenterAndExpand">                        <Label                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProRegular}"                            FontSize="18"                            HorizontalOptions="StartAndExpand"                            Text="{x:Static helpers:AppResources.TotalPrice}"                            TextColor="Black"                            VerticalOptions="Center" />                        <Label                            x:Name="TotalPrice"                            Margin="0,0,10,0"                            FontAttributes="Bold"                            FontFamily="{DynamicResource VerdanaProSemiBold}"                            FontSize="22"                            HorizontalOptions="EndAndExpand"                            TextColor="Black"                            VerticalOptions="CenterAndExpand" />                    </StackLayout>                </pancakeView:PancakeView>                <StackLayout                    Padding="5"                    VerticalOptions="Center"                    HorizontalOptions="FillAndExpand"                    Orientation="Horizontal">                    <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                        <StackLayout Orientation="Horizontal">                        <Label Style="{DynamicResource NormalLabelStyle}"  VerticalOptions="Center" Margin="5" Text="{x:Static helpers:AppResources.SelectAddress}" />                            <renderers:BorderlessPicker HorizontalOptions="End"                                 x:Name="AddressPicker"                                ItemDisplayBinding="{Binding AddressDetail}"                                Title="{x:Static helpers:AppResources.SelectAddress}"                                Style="{DynamicResource BorderlessPicker}"                                                                FontSize="15"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                TitleColor="{DynamicResource WhiteDarkFontColor}"                               />                            <Image Style="{DynamicResource ImageStyleEntry}" HorizontalOptions="End">                                <Image.Source>                                    <FontImageSource                                                                                                                    FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.ThinDown}"                                        Size="40"                                        Color="Black" />                                                                  </Image.Source>                                                               </Image>                        </StackLayout>                    </pancakeView:PancakeView>                 <!--  <pancakeView:PancakeView Style="{DynamicResource PancakeFrameStyle}">                        <pancakeView:PancakeView.GestureRecognizers>                            <TapGestureRecognizer Tapped="AddAddressClick" />                        </pancakeView:PancakeView.GestureRecognizers>                        <StackLayout Orientation="Horizontal">                            <Label                                Margin="5"                                FontFamily="{DynamicResource VerdanaProRegular}"                                FontSize="15"                                HorizontalOptions="StartAndExpand"                                Text="{x:Static helpers:AppResources.AddAddress}"                                TextColor="{DynamicResource WhiteDarkFontColor}"                                VerticalOptions="Center" />                            <Image                                Aspect="AspectFit"                                HeightRequest="20"                                HorizontalOptions="Start"                                VerticalOptions="CenterAndExpand"                                WidthRequest="20">                                <Image.Source>                                    <FontImageSource                                        FontFamily="{DynamicResource MaterialFontFamily}"                                        Glyph="{x:Static font:FontAwesomeIcons.Plus}"                                        Size="20"                                        Color="Black" />                                </Image.Source>                            </Image>                        </StackLayout>                    </pancakeView:PancakeView>-->                </StackLayout>                <StackLayout  VerticalOptions="EndAndExpand">                    <!--<Frame                    Margin="0,0,0,20"                    BackgroundColor="#015598"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueWithPaypal" />                    </Frame.GestureRecognizers>                        <Image                            Grid.Column="0"                            Aspect="AspectFill"                            HorizontalOptions="CenterAndExpand"                            VerticalOptions="CenterAndExpand">                            <Image.Source>                                <FontImageSource                                    FontFamily="{DynamicResource MaterialFontFamily}"                                    Glyph="Paytm"                                    Size="20"                                    Color="{DynamicResource LabelTextColor}" />                            </Image.Source>                        </Image>                </Frame>-->                <Frame                    Margin="0,0,0,400"                    BackgroundColor="#E59500"                    Style="{DynamicResource ButtonFrameStyle}">                    <Frame.GestureRecognizers>                        <TapGestureRecognizer Tapped="ContinueClick" />                    </Frame.GestureRecognizers>                    <Label                        Style="{DynamicResource ButtonLabelStyle}"                        Text="Cash On Delivery"                        FontSize="20"                        TextColor="{DynamicResource LabelTextColor}">                        <Label.GestureRecognizers>                            <TapGestureRecognizer Tapped="ContinueClick" />                        </Label.GestureRecognizers>                    </Label>                </Frame>                </StackLayout>            </StackLayout>        </ScrollView>    </ContentPage.Content></base:BasePage>