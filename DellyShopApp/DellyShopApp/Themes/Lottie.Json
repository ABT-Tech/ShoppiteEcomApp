                    frames: [1501, 1899],
                    speed: 1
                },
                {
                    state: 'loop',
                    transition: 'none',
                    frames: [1900, 2237],
                    speed: 1,
                },
                {
                    state: 'loop',
                    transition: 'none',
                    frames: [2238, 2698],
                    speed: 1,
                },
                {
                    state: 'loop',
                    transition: 'none',
