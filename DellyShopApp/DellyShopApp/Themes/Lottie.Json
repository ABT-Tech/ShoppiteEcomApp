    <script>

    const el = document.querySelector('.lf-homepage-features')
    const lazyloadImages = document.querySelectorAll(".lazy");
    const slugs = ["marketing", "interfaceDesign", "motionDesign", "development"];
    const slugsIndices = slugs.reduce((acc, slug, index) =>
        Object.assign(acc, {[slug]: index}), {}
    )

    let domDotLottiePlayerLoaded = false;
    let featureSliderPos = 1;
    let featureMobileSliderPos = 1;
    let numberOfPages = 4;
    let slideTimer = null;
    let slideMobileTimer = null;
    let activeTimer = null;
    let jumpTimer = null;
    let currentTab = 'marketing';
    let mobileLoaded = false;
    let firstSVGCount = true;
    let first = 1;
    let last = 316;
    let dotlottie = document.getElementById('lottie-player');
    let mobileDotlottie = document.getElementById('lottie-mobile-player');
    let lottiePlayer = null;
    let featureContainer = document.getElementById('feature-container');
    let featureMobileContainer = document.getElementById('lottieDiv');
    let transitionEndEventName = getTransitionEndEventName();

    const deviceType = () => {
        const ua = navigator.userAgent;
        if (/(tablet|ipad|playbook|silk)|(android(?!.*mobi))/i.test(ua)) {
            return "tablet";
        }
        else if (/Mobile|Android|iP(hone|od)|IEMobile|BlackBerry|Kindle|Silk-Accelerated|(hpw|web)OS|Opera M(obi|ini)/.test(ua)) {
            return "mobile";
        }
        return "desktop";
    };

    const device = deviceType();

    function swapDiv(to) {
        let newNodePosition = document.querySelector('.feature-mobile-position-'+currentTab+'-'+(to+1));
        featureContainer.insertBefore(featureMobileContainer, newNodePosition);
    }

    document.querySelector('.input-label-'+currentTab).setAttribute("style", "font-family: averta_stdbold; font-weight: 400;");
    document.querySelector('.label-'+currentTab).classList.remove('hidden');
    document.querySelectorAll('.feature-'+currentTab).forEach(function(e) {
            e.classList.remove('hidden');
        });
    document.querySelectorAll('.feature-'+currentTab+'.justify-center').forEach(function(e) {
            e.classList.add('flex');
        });

    swapDiv(1);

    if(device == 'desktop') {
        document.querySelector('.features-lottie').classList.remove('hidden');
    }

    dotlottie.addEventListener('ready', () => {

        document.querySelectorAll('.firstSVG').forEach(function(e) {
            e.classList.add('hidden');
        });

        if(device != 'desktop') {
            document.getElementById('lottieDiv').setAttribute("style","height: 282px; width: 100%; transition: all 0.4s linear");
            mobileDotlottie = mobileDotlottie.getLottie();
        } else {
            dotlottie = dotlottie.getLottie();
        }

        initializeLottiePlayer(device == 'desktop' ? dotlottie : mobileDotlottie);
        })

    //lazy load logos
    window.onload = function() {
        lazyloadImages.forEach(function(img) {

            img.src = img.dataset.src;
            var imageUrl = img.dataset.src;
            img.setAttribute("src", imageUrl);

            img.classList.remove('lazy');
        });
    };


    //observe viewport
    const observer = new window.IntersectionObserver(([entry]) => {
    if (entry.isIntersecting) {

        document.querySelector('.label-'+currentTab).classList.remove('hidden');

        document.querySelector('.input-label-'+currentTab).setAttribute("style", "font-family: averta_stdbold; font-weight: 400;");
        document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.add('page-active');

        startSlider();

        if(device != 'desktop') {
            mobileLoaded = true;
        }

        return
    }
            clearInterval(slideTimer);
            clearTimeout(activeTimer);
            clearTimeout(jumpTimer);
            clearTimeout(slideMobileTimer);

            document.querySelector('.page-'+currentTab+'-'+featureSliderPos).style.backgroundColor = "#C4C4C4";

            clearAllActivePages(currentTab);

            featureSliderPos = 1;
            slideTimer = null;
            swapDiv(featureSliderPos);
            jump(featureSliderPos);

    }, {
    root: null,
    threshold: 0.1, // set offset 0.1 means trigger if atleast 10% of element in viewport
    })

    observer.observe(el);

    function initializeLottiePlayer(player)
    {
