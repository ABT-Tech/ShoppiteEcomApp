    function next() {
    lottiePlayer.nextInteraction();
    }

    function jump(page) {

        let multiplier = slugsIndices[currentTab];
        let results = multiplier *4 + page;

        if(lottiePlayer) {
            lottiePlayer.jumpToInteraction(results-1);
        }
    }

    function setTab(tab) {
        clearTimeout(slideMobileTimer);
        clearInterval(slideTimer);
        clearTimeout(activeTimer);
        clearTimeout(jumpTimer);

        document.querySelector('.label-'+currentTab).classList.add('hidden');
        document.querySelector('.input-label-'+currentTab).removeAttribute("style");

        document.querySelectorAll('.feature-'+currentTab).forEach(function(e) {
            e.classList.add('hidden');
        });

        document.querySelectorAll('.feature-'+currentTab+'.justify-center').forEach(function(e) {
            e.classList.remove('flex');
        });

        clearAllActivePages(currentTab);

        featureSliderPos = 1;
        currentTab = tab;

        clearAllActivePages(currentTab);

        document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.add('page-active');
        document.querySelector('.label-'+currentTab).classList.remove('hidden');
        document.querySelector('.input-label-'+currentTab).setAttribute("style", "font-family: averta_stdbold; font-weight: 400;");

        document.querySelectorAll('.feature-'+currentTab).forEach(function(e) {
            e.classList.remove('hidden');
        });

        document.querySelectorAll('.feature-'+currentTab+'.justify-center').forEach(function(e) {
            e.classList.add('flex');
        });

        if(device != 'desktop') {
            let multiplier = slugsIndices[currentTab];
                let results = multiplier *4 + featureSliderPos;

                swapDiv(featureSliderPos);

                jump(featureSliderPos);
                document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.add('page-active');

                document.getElementById('lottieDiv').setAttribute("style", "height: 282px; width: 100%; transition: all 0.4s linear;");
        } else {
            jump(featureSliderPos);
        }


        startSlider();
    }

    function clearAllActivePages(tab) {

        for(let i = 1; i <= numberOfPages; i++) {
            document.querySelector('.page-'+tab+'-'+i).classList.remove('page-active');
        }
    }

    function resetSlider() {
        if(featureSliderPos === 4){
            featureSliderPos = 0;
        }
    }

    function startSlider() {
        if(device != 'desktop') {
            slideMobileTimer = setTimeout(() => {
                nextSlide();
            }, 8000);
        } else {
            slideTimer = setInterval(() => {
                nextSlide();
            }, 8000);
        }
    }

    function continueSlider(page, tab) {
        document.querySelector('.page-'+tab+'-'+page).style.backgroundColor = "#C4C4C4";
    }

    function nextSlide() {
        removeActive();
        resetSlider();
        featureSliderPos++;
        addActive();
    }

    function removeActive() {
        if(device != 'desktop') {

            document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.remove('page-active');
            document.getElementById('lottieDiv').setAttribute("style", "height: 0; width: 100%; transition: 0.4s linear;");
        } else {
            document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.remove('page-active');
        }
    }

    function getTransitionEndEventName() {
    var transitions = {
        "transition"      : "transitionend",
        "OTransition"     : "oTransitionEnd",
        "MozTransition"   : "transitionend",
        "WebkitTransition": "webkitTransitionEnd"
    }
    let bodyStyle = document.body.style;
    for(let transition in transitions) {
        if(bodyStyle[transition] != undefined) {
            return transitions[transition];
        }
    }
    }

    function addActive() {

        if(device != 'desktop') {
            clearTimeout(slideMobileTimer);


            document.getElementById('lottieDiv').addEventListener(transitionEndEventName, () => {

                swapDiv(featureSliderPos);
                activeTimer = setTimeout(() => {

                    clearTimeout(activeTimer);

                    document.getElementById('lottieDiv').setAttribute("style", 'height: 282px; width: 100%; transition: all 0.4s linear;');



                }, 100);

            });

            jumpTimer = setTimeout(() => {
                jump(featureSliderPos);
                // startSlider();
                clearTimeout(jumpTimer);

                document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.add('page-active');

                startSlider();
            }, 400);


        } else {
            document.querySelector('.page-'+currentTab+'-'+featureSliderPos).classList.add('page-active');
            jump(featureSliderPos);
        }
    }

    const block = document.querySelector('#amazing-ul-block');
    const horizontalScrollBar = document.querySelector('.tabs');
    const scollbar = document.querySelector('.fade');
    const clientWidth = horizontalScrollBar.clientWidth;

    horizontalScrollBar.addEventListener("scroll", (e) => {

        if(horizontalScrollBar.scrollLeft >= (clientWidth/3)) {
            scollbar.classList.add('hidden');
        }
        else {
            scollbar.classList.remove('hidden');
        }
    });

    function goToPage(page, tab) {
        if(device == 'desktop') {
            document.querySelector('.page-'+tab+'-'+page).style.backgroundColor = "#000000";
        }
    }

    function goToVisual(page, tab) {

        if(page != featureSliderPos || tab != currentTab) {
            if(device != 'desktop') {
                clearTimeout(slideMobileTimer);
            }

            removeActive();

            if(slideTimer) {
                clearInterval(slideTimer);
            }

            featureSliderPos = page;
            currentTab = tab;

            addActive();

            if(device == 'desktop') {
                startSlider();
            }
        }
    }
</script>



