    <script>

        
        let navShadows = true;

        document.querySelector('.m-nav-search').addEventListener('click', ()=>{
            document.querySelector('.lf-header').classList.add('mobile-search-active');
            document.body.classList.add('noscroll');
        });

        document.querySelector('.m-nav-hamburger').addEventListener('click', ()=>{
            document.querySelector('.lf-header').classList.add('mobile-nav-active');
            document.body.classList.add('noscroll');
        });

        document.querySelector('.m-nav-close').addEventListener('click', ()=>{
            document.querySelector('.lf-header').classList.remove('mobile-nav-active');
            document.querySelector('.lf-header').classList.remove('mobile-search-active');
            document.body.classList.remove('noscroll');
        });

        window.addEventListener('resize', ()=>{
            resetNavMenu();
            resetUserMenu();
        });

        document.addEventListener('scroll', ()=>{
            if(navShadows && document.querySelector('body').getBoundingClientRect().y<0){
                document.querySelector('.lf-header-container').classList.add('shadow-sm');
            }else{
                document.querySelector('.lf-header-container').classList.remove('shadow-sm');
            }
        });

        let userMenuList = document.querySelector('.user-menu-list');
        let menuItems = document.querySelectorAll('.nav-list-item');
        let activeMenuId = null;

        function resetUserMenu(){
            if(userMenuList){
                userMenuList.classList.remove('active');
            }
        }

        function resetNavMenu(){
            activeMenuId = null;
            menuItems.forEach((item)=>{
                item.classList.remove('active');
            });
        }

        function toggleMenuNav(id){
            resetUserMenu();
            if(window.innerWidth<768) {
                if (activeMenuId === id) {
                    activeMenuId = null;
                    document.querySelector('.nav-list-item-' + id).classList.toggle('active');
                } else if (activeMenuId !== null) {
                    document.querySelector('.nav-list-item-' + activeMenuId).classList.toggle('active');
                    document.querySelector('.nav-list-item-' + id).classList.toggle('active');
                    activeMenuId = id;
                } else {
                    document.querySelector('.nav-list-item-' + id).classList.toggle('active');
                    activeMenuId = id;
                }
            }
        }

        function toggleUserNav(){
            resetNavMenu();
            if(window.innerWidth<768) {
                document.querySelector('.user-menu-list').classList.toggle('active');
            }
        }

        function logout(){
            fetch("https:\/\/accounts.lottiefiles.com\/auth\/"+"self-service/logout/browser",{
                method: "GET",
                credentials: 'include',
            }).then( response => {
                response.json().then(logoutResponse =>{
                    window.location.href = logoutResponse.logout_url + '&return_to=' + "https:\/\/lottiefiles.com\/";
                })
            })
        }

    </script>

    
    <script>
    document.querySelector('html').addEventListener('dragenter', function(e){
        e.preventDefault();
    });
    document.querySelector('html').addEventListener('dragleave', function(e){
        e.preventDefault();
    });
    document.querySelector('html').addEventListener('dragover', function(e){
        e.preventDefault();
    });
    document.querySelector('html').addEventListener('drop', function(e){
        e.preventDefault();
        window.location.href = "https://app.lottiefiles.com/" + "preview";
    });
</script>
